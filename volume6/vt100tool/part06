echo Inspecting for damage in transit...
temp=/tmp/sharin$$; dtemp=/tmp/sharout$$
trap "rm -f $temp $dtemp; exit" 0 1 2 3 15
cat > $temp <<\!!!
     152    1451    9215 uu.widebot.r.16
     152    1854    9215 uu.widetop.b.16
     152    1878    9216 uu.widetop.bg.16
     152    1949    9215 uu.widetop.g.16
     152    1940    9215 uu.widetop.r.16
      86     723    5171 uu.cim_change.o
      52     290    1984 README
      34      39    1933 vshelltool.icon
     932   10124   55164 total
!!!
wc  fontdir/uu.widebot.r.16 fontdir/uu.widetop.b.16 fontdir/uu.widetop.bg.16 fontdir/uu.widetop.g.16 fontdir/uu.widetop.r.16 libdir/uu.cim_change.o src/README vshelltool.icon | sed 's=[^ ]*/==' | diff -b $temp - >$dtemp
if test -s $dtemp
then echo "Ouch [diff of wc output]:" ; cat $dtemp
else echo "No problems found."
fi
exit 0
