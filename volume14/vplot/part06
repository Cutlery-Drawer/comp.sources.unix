X    int             FONT, PREC, OVLY;
X{
X    vp_pc._font = FONT;
X    vp_pc._prec = PREC;
X    vp_pc._ovly = OVLY;
X
X    putc (VP_TXFONTPREC, vp_pc._pltout);
X    puth (vp_pc._font, vp_pc._pltout);
X    puth (vp_pc._prec, vp_pc._pltout);
X    puth (vp_pc._ovly, vp_pc._pltout);
