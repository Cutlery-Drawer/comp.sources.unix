foreach file (*.mn ../filters/*lib/*.mn)
set rootname = $file:t
set name = $rootname:r
if ( ! -e /usr/man/man9/$name.9 ) then
if ( -e `pwd`/$file ) then
if ( -f `pwd`/$file ) then
rm -f /usr/man/man9/$name.9
ln -s `pwd`/$file /usr/man/man9/$name.9
endif
endif
endif
end
END_OF_FILE
