X		xp = 1.8 + .9 * (i-1)
X		yp = .4 + .5 * (i-1)
X		xpath = .2 + (7. - i) / 9.
X		ypath = .2
X		xup = -.15
X		yup = .35
X		call vpcolor(mod(i,6)+2)
X		call vptfont((i-1),2,3)
X		call vpugtext
X     1         (xp,yp,xpath,ypath,xup,yup,'> SEP Geophysics <')
X	enddo
c
c  end of session
c
X	call vpendplot()
X	end
END_OF_FILE
