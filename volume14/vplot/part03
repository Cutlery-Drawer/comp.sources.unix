X *	Please log any further modifications made to this file:
X */
X
X#include "rtekpen.h"
X
rtekpoint (x, y)
X    int             x, y;
X{
X
X    rtekplot (x, y, 0);
X    CHKLEN (1);
X    byte (POINT);		/* cmdlen = 1 */
X}
END_OF_FILE
