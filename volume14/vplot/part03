X
X#include "../include/closestat.h"
X#include "vp.h"
X
vpclose (status)
X    int             status;
X{
X    switch (status)
X    {
X    case CLOSE_DONE:
X	vp_endplot ();
X	break;
X    }
X}
END_OF_FILE
