X#include <vplot.h>
X#include <math.h>
X
X#define NP 7
X
main ()
X{
float           xp, yp;
int             i;
X
X/*
X * Set up where you want the vplot output to go.
X */
X    vp_filep (stdout);
X
X/*
X * Decide what "style" plot this is to be.
X */
X    vp_style (STANDARD);
X
X/*
X * text
X */
X    for (i = 0; i < NP; i++)
X    {
X	xp = 1. + (7. / NP) * i;
X	yp = xp;
X	vp_color (i + 1);
X	vp_tfont (i * 2, STRING, OVLY_NORMAL);
X	vp_text (xp, yp, 15, 0, "This is a test...");
X/*	vp_utext(xp,yp,15,0,"This is a test..."); */
X    }
X
X/*
X * Finish up
X */
X    vp_endplot ();
X}
END_OF_FILE
