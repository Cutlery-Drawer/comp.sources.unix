X/*
X *
X *  source file:   ./filters/cteklib/ctekreset.c
X *
X * Joe Dellinger (SEP), June 11 1987
X *	Inserted this sample edit history entry.
X *	Please log any further modifications made to this file:
X */
X
X/* 
X * Put the terminal into standard configuration.
X */
X#include <stdio.h>
X#include "../include/attrcom.h"
X#include "../include/params.h"
X#include "ctek.h"
X
extern FILE    *pltout;
extern int      dev_xmax, dev_ymax, dev_xmin, dev_ymin;
X
ctekreset ()
X{
X    /*
X     * Set the graphics window. 
X     */
X    cteksetmode (TEK);
X    fprintf (pltout, "%cRW", ESC);
X    ctekxypack (dev_xmin, dev_ymin);
X    ctekxypack (dev_xmax, dev_ymax);
X
X    /*
X     * Turn off dashed lines. Set color to default color (usually white, but
X     * controlled by color table). 
X     */
X    cteklinestyle (0);
X    ctekattributes (SET_COLOR, DEFAULT_COLOR, 0, 0, 0);
X}
END_OF_FILE
