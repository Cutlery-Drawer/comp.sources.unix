X
X/*
X *
X *  source file:   ./filters/gigilib/gigipoly.c
X *
X * Joe Dellinger (SEP), June 11 1987
X *	Inserted this sample edit history entry.
X *	Please log any further modifications made to this file:
X * Joe Dellinger Feb 16 1988
X *	Fixed bug in call to gigiplot
X */
X
X#include <stdio.h>
X#include "gigi.h"
extern FILE    *pltout;
X
int             in_a_poly = 0;
X
static int      need_set_shade = 0;
X
gigistartpoly (npts)
X    int             npts;
X{
X    lost = 1;
X    need_set_shade = 1;
X    in_a_poly = 1;
X    if (lastop != 'P')
X	fprintf (pltout, "P");
X}
X
gigimidpoly (x, y)
X    int             x, y;
X{
X    gigiplot (x, y, 0);
X    if (need_set_shade == 1)
X    {
X	need_set_shade = 0;
X	fprintf (pltout, "V@W(W(S1))");
X	lastop = 'V';
X    }
X}
X
gigiendpoly (last)
X    int             last;
X{
X    fprintf (pltout, "(W(S0))V@W");
X    lost = 1;
X    in_a_poly = 0;
X}
END_OF_FILE
