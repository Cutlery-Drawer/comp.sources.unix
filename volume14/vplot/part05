X#define X		x
X#define Y		y
X#endif UUU
X
X#endif
X
X#ifndef UUU
DRAW (x, y)
X#else UUU
UDRAW (x, y)
X#endif UUU
X    float           X, Y;
X{
X#ifndef UUU
X    vp_plot (X, Y, 1);
X#else UUU
float           xp, yp;
X    xp = vp_pc._x0 + (X - vp_pc._xu0) * vp_pc._xscl;
X    yp = vp_pc._y0 + (Y - vp_pc._yu0) * vp_pc._yscl;
X    vp_plot (xp, yp, 1);
X#endif UUU
X}
END_OF_FILE
