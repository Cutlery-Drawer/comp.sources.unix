X * Joe Dellinger (SEP), June 11 1987
X *	Inserted this sample edit history entry.
X *	Please log any further modifications made to this file:
X */
X
X/*
X * Device independent subroutine to handle message operations
X */
X#include	<stdio.h>
X#include	"../include/mesgcom.h"
X#include	"../include/enum.h"
X
genmessage (command, string)
X    int             command;
X    char            string[];
X{
X
X    switch (command)
X    {
X    case MESG_HOME:
X    case MESG_READY:
X	fflush (stderr);
X	break;
X    case MESG_TEXT:
X	fprintf (stderr, "%s", string);
X	break;
X    case MESG_HIGHLIGHT_ON:
X	/* Beep at them to get their attention */
X	fprintf (stderr, "\07\07");
X	break;
X    case MESG_ON:
X    case MESG_OFF:
X    case MESG_ERASE:
X    case MESG_HIGHLIGHT_OFF:
X    case MESG_DONE:
X    default:
X	fflush (stderr);
X	break;
X    }
X}
END_OF_FILE
