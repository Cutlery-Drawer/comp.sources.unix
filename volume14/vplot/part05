sed "s/^X//" >'Vplot_Kernel/filters/Tests/markertest.f' <<'END_OF_FILE'
c==============================================================
c	FORTRAN routine displaying polymarkers
c==============================================================
c
X	dimension xpoly(20),ypoly(20)
X	integer marktype(8),color(8)
X	integer   out
X	data marktype /2, 3, 4, 5, 20, 21, 22, 23/
X	data color /0, 1, 2, 3, 4, 5, 6, 7/
c
c  set vplot output file
c
X	call vpfile('out.vp')
c
c  choose standard style for vplot
c
X	call vpstyle(STANDARD)
c
c  set user window
c
X	xmin = 0.
X	ymin = 0.
X	xmax = 100.
X	ymax = 100.
X	call vpstretch(xmin,ymin,xmax,ymax)
X
X
X	ncol   	= 4
X	nrow	= 8
X	dcol	= (xmax-xmin)/(ncol+1)
X	drow	= (ymax-ymin)/(nrow+1)
X	isiz	= 2
X	icolor  = 0
c
c  compute positions of markers
c
X	do irow = 1,nrow
X	   ix = 0
X	   iy = 0
X	   ypos = ymin + drow*irow
c
c  set marker type,color and size
c
X	   ityp = ityp+1
X	   mtype= marktype(ityp)
X	   icolor = icolor+1
X	   isiz = isiz+2
X	   do icol =1,ncol
X		ix = ix+1
X		iy = iy+1
X		xpoly(ix) = xmin + dcol*icol
X		ypoly(iy) = ypos
X	   end do
c
c  display markers
c
X	   call vpcolor(color(icolor))
X	   call vpupmark(ncol,mtype,isiz,xpoly,ypoly)
X	end do
c
c  end of session
c
X	call vpendplot()
X	end
X
X
END_OF_FILE
