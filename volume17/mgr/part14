X#define MAXSHELL	128		/* max chars read from shell at once */
X#define POLL_INT	0		/* us's of pause at each select */
X#define MAXNAME		35		/* max argv[0] length */
X#define MAXTTY		35		/* max tty device name length */
X#define MAXFONT		100		/* # of different fonts */
X#define MAXMENU		20		/* max number of menus per window */
X#define MAXBITMAPS	50		/* max number of bitmaps per window */
X#define MAXITEMS	200		/* 2 * max # of menu items */
X#define MAXEVENTS	22		/* max number of events, -4 thru 17,
X					see event.h */
X#define MIN_Y		1		/* minimum # of rows in a window */
X#define MIN_X		5		/* min # of columns in a window */
X#define MAX_PATH	100		/* max path length for font file */
X#define GMAX		1000L		/* max graphics coordinate */
X#define MOUSE_BUFF	30		/* size of mouse input buffer */
X#define MSG_MODEMASK	02		/* invalid permission mask for tty */
X
X#ifdef COLOR
X#define SCREEN_DEV	"/dev/fb"	/* where to find the frame buffer */
X#else
X#define SCREEN_DEV	"/dev/bwtwo0"	/* where to find the frame buffer */
X#endif
