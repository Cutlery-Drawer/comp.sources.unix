X	rm -rf $(INSDIR)/ReMoveThisFile
X
X$(INSDIR):
X	mkdir $@
X
Xclean clobber:
X
Xlist:
X	@for i in Makefile .mgrc $(FONTS); do \
X		echo "${START}/$$i"; \
X	done	
X
Xcpio:
X	make -s list | cpio -ocv > ${CPIO}
END_OF_FILE
# end of 'font-32/Makefile'
fi
