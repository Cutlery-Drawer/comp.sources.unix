X(setq shape-vlog-command (concat shape-bin-dir "vlog"))
X(setq shape-wishes-command (concat shape-interface-dir "shape-wishes"))
X
X(load (concat shape-interface-dir "emacs-shape-autoloads.el") t t)
X
