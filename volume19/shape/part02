X/* This is the main program for a simple demo of shape */
X
X#include <stdio.h>
X
Xmain()
X{
X#ifdef DEBUG
Xprintf("main program with DEBUG\n");
X#endif DEBUG
X  printf("main program $__name$.$__type$[$__version$]\n\n");
X  
X  printf("    ****   *   *    *    ****   ****    \n");
X  printf("   *    *  *   *   * *   *   *  *       \n");
X  printf("   *       *   *  *   *  *   *  *       \n");
X  printf("    ****   *****  *****  ****   ***     \n");
X  printf("        *  *   *  *   *  *      *       \n");
X  printf("   *    *  *   *  *   *  *      *       \n");
X  printf("    ****   *   *  *   *  *      ****    \n");
X  printf("\n");
Xtitle();
Xcomponents();
X}
END_OF_FILE
