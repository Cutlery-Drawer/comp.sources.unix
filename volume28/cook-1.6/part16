Xecho checking for minix/config.h
Xcat > conftest.c <<EOF
X#include "confdefs.h"
X#include <minix/config.h>
XEOF
Xerr=`eval "($CPP conftest.c >/dev/null) 2>&1"`
Xif test -z "$err"; then
X  rm -rf conftest*
X  MINIX=1
