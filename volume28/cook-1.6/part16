Xecho "#define" _POSIX_SOURCE 1 >> confdefs.h
XDEFS="$DEFS -D_POSIX_SOURCE=1"
XSEDDEFS="${SEDDEFS}\${SEDdA}_POSIX_SOURCE\${SEDdB}_POSIX_SOURCE\${SEDdC}1\${SEDdD}
