X  
X{
Xtest -n "$verbose" && \
Xecho "	defining _MINIX"
Xecho "#define" _MINIX 1 >> confdefs.h
XDEFS="$DEFS -D_MINIX=1"
XSEDDEFS="${SEDDEFS}\${SEDdA}_MINIX\${SEDdB}_MINIX\${SEDdC}1\${SEDdD}
