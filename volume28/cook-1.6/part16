Xecho "#define" _POSIX_1_SOURCE 2 >> confdefs.h
XDEFS="$DEFS -D_POSIX_1_SOURCE=2"
XSEDDEFS="${SEDDEFS}\${SEDdA}_POSIX_1_SOURCE\${SEDdB}_POSIX_1_SOURCE\${SEDdC}2\${SEDdD}
