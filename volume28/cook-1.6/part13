if test 20796 -ne `wc -c <'aux/CHANGES.1.6'`; then
    echo shar: \"'aux/CHANGES.1.6'\" unpacked with wrong size!
fi
# end of 'aux/CHANGES.1.6'
fi
if test -f 'aux/Howto.cook' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'aux/Howto.cook'\"
else
echo shar: Extracting \"'aux/Howto.cook'\" \(18409 characters\)
sed "s/^X//" >'aux/Howto.cook' <<'END_OF_FILE'
X/*
X *	cook - file construction tool
X *	Copyright (C) 1991, 1992, 1993, 1994 Peter Miller.
X *	All rights reserved.
X *
X *	This program is free software; you can redistribute it and/or modify
X *	it under the terms of the GNU General Public License as published by
X *	the Free Software Foundation; either version 2 of the License, or
X *	(at your option) any later version.
X *
X *	This program is distributed in the hope that it will be useful,
X *	but WITHOUT ANY WARRANTY; without even the implied warranty of
X *	MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the
X *	GNU General Public License for more details.
X *
X *	You should have received a copy of the GNU General Public License
X *	along with this program; if not, write to the Free Software
X *	Foundation, Inc., 675 Mass Ave, Cambridge, MA 02139, USA.
X *
X * MANIFEST: instructions to cook, how to build project
X */
X
X/*
X * Update file times if necessary, to ensure outputs are newer then inputs.
X * On fast machines, that happens frequently for transitive dependencies.
X */
Xset update;
X
X/*
X * You may want to change some of these options.
X * ([debug] must be defined, even if empty, use comments.)
X *
X * If you want to turn the DEBUG compile flag on,
X * edit the common/main.h file.
X */
Xdebug = /* -g */ ;
Xoption_flags = -D\'LIBDIR\=\"/usr/local/lib/cook\"\';
X
X/*
X * Make sure the [project], [change], [version] and [arch] variables are set.
X */
X#ifndef project
Xecho "The [project] variable was not set from the command line." set silent;
Xecho "If you want to do a build which is not a development build" set silent;
Xecho "or an integration build, use the make command, not cook." set silent;
Xfail;
X#endif
X#ifndef change
Xecho "The [change] variable was not set from the command line." set silent;
Xecho "If you want to do a build which is not a development build" set silent;
Xecho "or an integration build, use the make command, not cook." set silent;
Xfail;
X#endif
X#ifndef version
Xecho "The [version] variable was not set from the command line." set silent;
Xecho "If you want to do a build which is not a development build" set silent;
Xecho "or an integration build, use the make command, not cook." set silent;
Xfail;
X#endif
X#ifndef arch
Xecho "The [arch] variable was not set from the command line." set silent;
Xecho "If you want to do a build which is not a development build" set silent;
Xecho "or an integration build, use the make command, not cook." set silent;
Xfail;
X#endif
X
Xarch_ed = -e "'s|^"[arch]"/|[arch]/|'";
Xbaseline_ed = ;
X
X/*
X * find the baseline
X */
Xif [match_mask %1C%2 [version]] then
X{
X	/* baseline = [collect aegis -cd -bl -p [project]]; */
X	baseline = bl;
X	search_list = . [baseline];
X	baseline_ed = -e "'s|^"[baseline]"/||'";
X}
X
Xversion_short = [fromto %1.%2.%3 %1.%2 [version]];
X
X/*
X * The default recipe
X */
Xall =
X	[arch]/common/config.h
X	doc.targets
X	[arch]/bin/c_incl
X	[arch]/bin/cook
X	[arch]/bin/cooktime
X	[arch]/bin/cookfp
X	[arch]/bin/find_libs
X	[arch]/bin/make2cook
X	[arch]/bin/roffpp
X	Makefile.in
X	;
X
X/*
X * Integration builds also make the shar files for distribution.
X */
Xif [not [defined baseline]] then
X	all = [all] archive/Part.01.gz archive/[project].tar.gz
X		archive/[project].patch.gz;
X
Xall: [all];
X
X/*
X * The source files.
X * This is given to us by aegis.
X */
Xvs_file = common/patchlevel.h;
Xchange_files =
X	[collect aegis -l chafil -ter -p [project] -c [change]]
X	[vs_file] doc/version.so
X	;
Xproject_files =
X	[collect aegis -l profil -ter -p [project] -c [change]]
X	;
Xsource_files =
X	[sort [stringset [project_files] [change_files]]]
X	;
Xcommon_obj = [arch]/common/lib.a [arch]/common/version-stmp.o;
Xlib_obj =
X	[stringset
X		[fromto
X			common/%0%.c
X			[arch]/common/%0%.o
X			[match_mask common/%0%.c [source_files]]
X		]
X	-
X		[common_obj]
X	];
Xcook_obj =
X	[fromto
X		cook/%0%.y
X		[arch]/cook/%0%.gen.o
X		[match_mask cook/%0%.y [source_files]]
X	]
X	[fromto
X		cook/%0%.c
X		[arch]/cook/%0%.o
X		[match_mask cook/%0%.c [source_files]]
X	]
X	[common_obj]
X	;
Xmake2cook_obj =
X	[fromto
X		make2cook/%0%.y
X		[arch]/make2cook/%0%.gen.o
X		[match_mask make2cook/%0%.y [source_files]]
X	]
X	[fromto
X		make2cook/%0%.c
X		[arch]/make2cook/%0%.o
X		[match_mask make2cook/%0%.c [source_files]]
X	]
X	[common_obj]
X	;
Xc_incl_obj =
X	[fromto
X		c_incl/%.c
X		[arch]/c_incl/%.o
X		[match_mask c_incl/%.c [source_files]]
X	]
X	[common_obj]
X	;
Xfind_libs_obj =
X	[fromto
X		find_libs/%.c
X		[arch]/find_libs/%.o
X		[match_mask find_libs/%.c [source_files]]
X	]
X	[common_obj]
X	;
Xroffpp_obj =
X	[fromto
X		roffpp/%.c
X		[arch]/roffpp/%.o
X		[match_mask roffpp/%.c [source_files]]
X	]
X	[common_obj]
X	;
Xtxt2c_obj =
X	[fromto
X		txt2c/%.c
X		[arch]/txt2c/%.o
X		[match_mask txt2c/%.c [source_files]]
X	]
X	[arch]/common/ansi.o
X	[arch]/common/arglex.o
X	[arch]/common/error.o
X	[arch]/common/mem.o
X	[arch]/common/mprintf.o
X	[arch]/common/str.o
X	[arch]/common/trace.o
X	;
Xcookfp_obj =
X	[fromto
X		cookfp/%.c
X		[arch]/cookfp/%.o
X		[match_mask cookfp/%.c [source_files]]
X	]
X	[common_obj]
X	;
Xcooktime_obj =
X	[fromto
X		cooktime/%.y
X		[arch]/cooktime/%.gen.o
X		[match_mask cooktime/%.y [source_files]]
X	]
X	[fromto
X		cooktime/%.c
X		[arch]/cooktime/%.o
X		[match_mask cooktime/%.c [source_files]]
X	]
X	[common_obj]
X	;
Xlib_srcs =
X	[match_mask lib/% [source_files]]
X	;
X
X/*
X * how to compile C sources
X */
Xcc = cc;
Xcc_flags = -O [debug];
Xcc_include_flags = ;
X
X/*
X * this next section is for gcc
X *	comment it out if you don't have gcc
X *
X * The gcc include files MUST be first.
X */
Xcc = gcc;
Xcc_flags = [cc_flags] -ansi -Wall -Wstrict-prototypes -Wmissing-prototypes;
X
X/*
X * include files from the common directory.
X */
Xcc_include_flags =
X	[cc_include_flags]
X	[prepost -I /[arch]/common [search_list]]
X	[prepost "-I" "/common" [search_list]]
X	;
X
X/*
X * Build %.o from %.c
X */
X[arch]/%1/%0%2.o: %1/%0%2.c
X{
X	if [not [exists [dirname [target]]]] then
X		mkdir -p [dirname [target]]
X			set clearstat;
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X	if [defined %2_flags] then
X		extra = [%2_flags];
X	else
X		extra = ;
X	[cc] [cc_flags] [extra] [stringset [cc_include_flags] [prepost "-I"
X		"/%1" [search_list]]] -c [resolve %1/%0%2.c]
X		['if' [in gcc [cc]] 'then' -o [target]];
X	if [not [in gcc [cc]]] then
X		mv %2.o [target];
X}
X
X%1/%0%2.d: %1/%0%2.c
X{
X	if [not [exists [dirname [target]]]] then
X		mkdir -p [dirname [target]]
X			set clearstat;
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X	c_incl -nc -ns -eia
X	    [stringset [cc_include_flags] [prepost "-I" "/%1" [search_list]]]
X	    -prefix "'[arch]/%1/%0%2.o %1/%0%2.c,M %1/%0%2.d:'"
X	    -suffix "'set nodefault;'" [resolve %1/%0%2.c]
X		| sed [baseline_ed] [arch_ed]
X		> [target];
X}
X
Xc-dep-files =
X	[fromto %0%.c %0%.d [match_mask %0%.c [source_files]]]
X	[fromto %0%.y %0%.gen.d [match_mask %0%.y [source_files]]]
X	;
X
X[c-dep-files]: [arch]/common/config.h;
X
X#include-cooked [c-dep-files]
X
X/*
X * A few explicit things, to make sure the .d files
X * are constructed correctly.  Sigh.
X */
Xdoc/refman.d:		doc/function.so;
Xc_incl/main.d:		[arch]/man1/c_incl.h;
Xcommon/help.d:		[arch]/man1/o__rules.h [arch]/man1/z_exit.h;
Xcook/fngrprnt_lex.d:	cook/fngrprnt.gen.h;
Xcook/lex.d:		cook/parse.gen.h cook/hashline.gen.h;
Xcook/main.d:		[arch]/man1/cook.h;
Xcookfp/main.d:		[arch]/man1/cookfp.h;
Xcooktime/main.d:	[arch]/man1/cooktime.h;
Xfind_libs/main.d:	[arch]/man1/find_libs.h;
Xroffpp/main.d:		[arch]/man1/roffpp.h;
X
X/*
X * How to use yacc sources.
X */
Xyacc = bison -y;
Xyacc_flags = ;
X
X%1/%2.gen.c %1/%2.gen.h: %1/%2.y
X{
X	if [not [exists [dirname [target]]]] then
X		mkdir [dirname [target]]
X			set clearstat;
X	rm -f %1/%2.list y.output
X		set silent;
X	if [exists %1/%2.gen.c] then
X		rm %1/%2.gen.c
X			set clearstat;
X	if [exists %1/%2.gen.h] then
X		rm %1/%2.gen.h
X			set clearstat;
X	[yacc] -d [yacc_flags] [resolve %1/%2.y];
X	sed -e \'s/\[yY\]\[yY\]/%2_/g\' y.tab.c > %1/%2.gen.c;
X	rm y.tab.c;
X	sed -e \'s/\[yY\]\[yY\]/%2_/g\' y.tab.h > %1/%2.gen.h;
X	rm y.tab.h;
X	if [exists y.output] then
X		mv y.output %1/%2.list
X			set clearstat;
X}
X
X
X/*
X * build the programs
X */
X[arch]/bin/%: [%_obj]
X{
X	if [not [exists [dirname [target]]]] then
X		mkdir -p [dirname [target]]
X			set clearstat;
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X
X	[cc] -o [target] [resolve [%_obj]];
X	chmod og\=u-w [target];
X}
X
X
X/*
X * The version stamp is to be updated for every 
X * integration and development build.
X * (The integrate_begin_command in the config file
X * deletes them for integration builds.)
X */
X
Xif [not [exists [vs_file]]] then
X{
X	if [not [exists [dirname [vs_file]]]] then
X		mkdir [dirname [vs_file]]
X			set clearstat;
X	echo "'#define PATCHLEVEL \""[version]"\"'" > [vs_file]
X		set clearstat;
X}
X
X#ifndef baseline
X
Xif [not [exists doc/version.so]] then
X{
X	if [not [exists doc]] then
X		mkdir doc
X			set clearstat;
X	echo "'.ds V) "[version]"'" > doc/version.so;
X	echo "'.ds v) "[version_short]"'" >> doc/version.so;
X	aegis -list version -p [project] -c [change]
X		| gawk -F'\'"\'' "'/previous/{print $2}'"
X		| gawk -F. "'{print \".ds w) \"$1\".\"$2}'"
X		>> doc/version.so;
X	cooktime -m 2-Jan-70 doc/version.so
X		set clearstat;
X}
Xif [not [exists aux/new.so]] then
X{
X	if [not [exists aux]] then
X		mkdir aux
X			set clearstat;
X	echo "'.so new."[version_short]".so'" > aux/new.so;
X	cooktime -m 2-Jan-70 aux/new.so
X		set clearstat;
X}
X
X#endif
X
X
X/*
X * These recipes build a Makefile for a user.  They do not build a
X * Makefile useful for a development build, because no arch
X * information is included in the Makefile.
X */
X
Xmakefile-per-file = [prepost "" ",M" [source_files]];
X
XMakefile.in: aux/Makefile.head aux/Makefile.sh aux/Makefile.awk [vs_file]
X	[makefile-per-file]
X{
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X	cat [resolve aux/Makefile.head [makefile-per-file]] > [target];
X	sh [resolve aux/Makefile.sh] [source_files]
X		| gawk -f [resolve aux/Makefile.awk]
X		>> [target];
X}
X
X%0%,M: aux/Makefi.file.sh aux/Makefile.awk
X{
X	if [not [exists [dirname [target]]]] then
X		mkdir -p [dirname [target]]
X			set clearstat;
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X	sh [resolve aux/Makefi.file.sh] %0% [resolve %0%]
X		| gawk -f [resolve aux/Makefile.awk]
X		> [target];
X}
X
XCHANGES = aux/CHANGES.[version_short];
X
Xsource_file_order =
X	README
X	[sort
X		[stringset
X			[CHANGES] [source_files]
X		-
X			README
X		]
X	]
X	;
X
Xarchive/Part.01.gz: [source_file_order]
X{
X	if [not [exists archive]] then
X		mkdir archive
X			set clearstat;
X	parts = [glob "archive/Part.*"];
X	if [parts] then
X		rm [parts]
X			set clearstat;
X	/* assumes shar.3.49 available */
X	shar -n[project] -a -c -oarchive/Part -L50
X		-s "'Peter Miller <pmiller@bmr.gov.au>'"
X		[resolve [source_file_order]];
X	gzip -v archive/Part.*;
X}
X
Xarchive/[project].tar.gz: [source_file_order]
X{
X	if [not [exists archive]] then
X		mkdir archive
X			set clearstat;
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X	tar cf - [resolve [source_file_order]] |
X	tardy -unu 0 -gnu 0 -una Peter -gna Miller -p [project] -ms 0644
X		-mc 07022 -now > archive/[project].tar;
X	gzip -v archive/[project].tar;
X}
X
X[CHANGES]: aux/CHANGES.sh [vs_file]
X{
X	if [not [exists aux]] then
X		mkdir aux
X			set clearstat;
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X	sh [resolve aux/CHANGES.sh] [project] > [target];
X}
X
X%: aux/%.man
X{
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X	roffpp [prepost "-I" "/aux" [search_list]] [resolve aux/%.man]
X		| groff -Tascii -t -P-hub -man > [target];
X}
X
Xaux/%.d: aux/%.man
X{
X	if [not [exists [dirname [target]]]] then
X		mkdir -p [dirname [target]]
X			set clearstat;
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X	c_incl -nc -ns -eia -r [prepost "-I" "/aux" [search_list]]
X	    -prefix "'% aux/%.d:'"
X	    -suffix "'set nodefault;'" [resolve aux/%.man]
X		| sed [baseline_ed] [arch_ed] > [target];
X}
X
Xaux-dep-files =
X	[fromto aux/%.man aux/%.d [match_mask aux/%.man [source_files]]]
X	;
X
X#include-cooked [aux-dep-files]
X
X/*
X * formatting documents and manual entries
X */
Xdoc.targets: cat txt special;
X
X/*
X * manual entries
X */
Xcat =
X	[fromto man%1/%2.%1 cat%1/%2.%1 [match_mask man%1/%2.%1 [source_files]]]
X	[fromto man1/%.1 man1/%.h [match_mask man1/%.1 [source_files]]]
X	[fromto man1/%.so man1/%.h [match_mask man1/%.so [source_files]]]
X	;
Xcat: [cat];
X
Xcat%1/%2.%1: man%1/%2.%1
X{
X	if [not [exists cat%1]] then
X		mkdir cat%1
X			set clearstat;
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X	roffpp [prepost "-I" "/man%1" [search_list]] [resolve man%1/%2.%1]
X		| groff -Tascii -t -P-h -man > [target];
X}
X
Xman%1/%2.d: man%1/%2.%1
X{
X	if [not [exists [dirname [target]]]] then
X		mkdir -p [dirname [target]]
X			set clearstat;
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X	c_incl -nc -ns -eia -r [prepost "-I" "/man%1" [search_list]]
X	    -prefix "'cat%1/%2.%1 man%1/%2.d:'"
X	    -suffix "'set nodefault;'" [resolve man%1/%2.%1]
X		| sed [baseline_ed] [arch_ed] > [target];
X}
X
Xman-dep-files =
X	[fromto man%1/%2.%1 man%1/%2.d [match_mask man%1/%2.%1 [source_files]]]
X	;
X
X#include-cooked [man-dep-files]
X
X
X/*
X * user manuals
X */
X
Xtxt = [fromto doc/%.t doc/%.ps.gz [match_mask doc/%.t [source_files]]];
Xtxt: [txt];
X
Xdoc/%.ps.gz: doc/%.t
X{
X	if [not [exists doc]] then
X		mkdir doc
X			set clearstat;
X	if [exists [target]] then
X		rm -f [target]
X			set clearstat;
X	roffpp [prepost "-I" "/doc" [search_list]] [resolve doc/%.t]
X		| groff -p -t -mm | gzip > doc/%.ps.gz; 
X}
X
Xdoc/%.d: doc/%.t
X{
X	if [not [exists [dirname [target]]]] then
X		mkdir -p [dirname [target]]
X			set clearstat;
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X	c_incl -nc -ns -eia -r [prepost "-I" "/doc" [search_list]]
X	    -prefix "'doc/%.ps.gz doc/%.d:'"
X	    -suffix "'set nodefault;'" [resolve doc/%.t]
X		| sed [baseline_ed] [arch_ed] > [target];
X}
X
Xdoc-dep-files =
X	[fromto doc/%.t doc/%.d [match_mask doc/%.t [source_files]]]
X	;
X
X#include-cooked [doc-dep-files]
X
X
X/*
X * specials
X */
X
Xspecial = [fromto aux/%.man % [match_mask aux/%.man [source_files]]];
Xspecial: [special];
X
X%: aux/%.man: [collect c_incl -r -eia [prepost "-I" "/aux" [search_list]]
X	[resolve aux/%.man]]
X{
X	if [exists [target]] then
X		rm -f [target]
X			set clearstat;
X	roffpp [prepost "-I" "/aux" [search_list]] [resolve aux/%.man]
X		| tbl | nroff -man > %; 
X}
X
XMANIFEST: [stringset [source_files] - MANIFEST] [CHANGES]
X{
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X	sh [resolve aux/MANIFEST.sh] [resolve [need]] > [target];
X}
X
Xpatch-files =
X	[prepost "" ",P"
X		common/patchlevel.h
X		[stringset
X			[source_file_order]
X		-
X			common/patchlevel.h
X		]
X	];
X
Xarchive/[project].patch.gz: aux/patches.sh [patch-files]
X{
X	if [not [exists archive]] then
X		mkdir archive
X			set clearstat;
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X	sh [resolve [need]] | gzip > [target];
X}
X
X%0%,P: %0%: aux/patch.file.sh
X{
X	if [not [exists [dirname [target]]]] then
X		mkdir -p [dirname [target]]
X			set clearstat;
X	if [exists [target]] then
X		rm [target]
X			set clearstat;
X	sh [resolve aux/patch.file.sh] [project] [change] %0% [resolve %0%]
X		> [target];
X}
X
