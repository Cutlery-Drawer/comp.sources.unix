LANG=""; export LANG
PATH=/bin:/usr/bin:$PATH; export PATH

echo x - codes.h
cat >codes.h <<'HIPPOPOTAMUS_ON_A_POGO_STICK'
#ifndef CODES_H
#define CODES_H

/*
 *	Grid codes.  These must be distinct.
 */
#define	I_BLANK		'.'	/* 12345 */	/* .. */
#define	I_BLANK2	':'	/* 12345 */	/* :: */
#define	I_BLANK3	'~'	/* 12345 */	/* ,' */
#define	I_STRIPES	'/'	/* 12345 */	/* /    / */
#define	I_SSTRIPES	'y'	/*  2345 */	/* // */
#define	I_DIAMONDS	'd'	/*  2... */	/* /  \ \  / */
#define	I_BLANK7	'k'	/* 12345 */	/* || */
#define	I_BLANK8	'q'	/* 12345 */	/* -- */
#define	I_BLANK9	'p'	/* 12345 */	/* ++ */
#define	I_BRICK		'r'	/* 12345 */	/* .: ;, */
#define	I_DARKBRICK	'b'	/*    45 */	/* -   - */
#define	I_CINDER	'c'	/*    45 */	/* |   | */
#define	I_SHADOW	'x'	/*    45 */	/*    */
#define	I_GLOOM		'g'	/*  2    */	/* .   . */
#define	I_STAR		'\''	/* 1   5 */	/*  .  star in night sky */
#define	I_PLEDGE	';'	/*  23   */	/* phantom ledge */

#define BLANKS	".;~/ydrxcbg:kqp'EGKS[]-*+%Z`"
#define CRUMBLEBLANKS ".;~/ydrxcbg:kqp'EGKS[]-*+%Z`&$"

#define I_BLOCK		'#'	/* 12345 */
#define I_PBLOCK	'Z'	/*     5 */	/* phony block */
#define	I_SKYBLOCK	'_'	/*     5 */	/* upper limit of sky */
#define	I_RATHOLE	'%'	/* 12345 */
#define	I_CRUMBOX	'&'	/* 12345 */
#define	I_CRUMBOX2	'$'	/* not used on input */
#define	I_TRAPLEFT	'['	/* 12345 */
#define	I_TRAPRIGHT	']'	/* 12345 */
#define	I_TRAPCENTER	'*'	/* 12345 */
#define	I_TRAPEDGE	'-'	/* 12345 */
#define	I_ENTRY		'`'	/* 12345 */	/* entrance in an antechamber */
#define	I_DOOR		'+'	/* 12345 */

#define	I_BALCONY	'T'	/* 12345 */
#define	I_HBALCONY	'"'	/*   3   */	/* hidden - appears if on it */
#define	I_HVBALCONY	'\\'	/* not used on input */	/* visible hid. balc. */
#define	I_IBALCONY	'e'	/*    45 */	/* invisible - never appears */
#define	I_FBALCONY	'a'	/*     5 */	/* top-of-fence balcony */
#define BALCONIES	"T\"\\ea"

#define	I_VPIPE		'H'	/* 12345 */
#define	I_HPIPE		'='	/* 12345 */
#define	I_ULPIPE	'J'	/* 12345 */
#define	I_URPIPE	'L'	/* 12345 */
#define	I_DLPIPE	'7'	/* 12345 */
#define	I_DRPIPE	'F'	/* 12345 */
#define	I_HEAVYBOX	'@'	/*  2345 */
#define I_MHPIPE	'm'	/*    4  */
#define I_MVPIPE	'h'	/*   345 */
#define I_MULPIPE	'j'	/*   34  */
#define I_MURPIPE	'l'	/*   3   */
#define I_MDRPIPE	'f'	/* 1     */
#define ITSAPIPE(x)	(strchr("H=JL7Fmhjlf",(x)))

#define	I_LBLOWER	'<'	/* 12345 */
#define	I_RBLOWER	'>'	/* 12345 */
#define	I_UBLOWER	'^'	/* 12345 */
#define	I_MUBLOWER	'n'	/*  2345 */
#define	I_MLBLOWER	'('	/*   345 */
#define	I_MRBLOWER	')'	/*  23 5 */
#define	ITSABLOWER(x)	(strchr("<>^n()",(x)))

#define	I_ELEVATOR	'E'	/* 12345 */
#define	I_GONDOLA	'G'	/*  234  */
#define	I_TRACK		'K'	/*     5 */	/* empty track */

#define I_MOVEBOX	' '	/* 12345 */
#define	I_SPRING	'I'	/*   345 */
#define	I_MSPRING	'i'	/*   345 */
#define	I_JAR		'U'	/*    45 */
#define	I_RATJAR	'\001'	/* not used on input */

#define	CARRYABLE	"mhjlfn() iU\1"

#define I_VIPER		'V'	/* 1 3 5 */
#define I_ALIEN		'A'	/*  2  5 */
#define I_PIG		'P'	/*   3 5 */
#define I_SPIG		'N'	/*   3   */	/* swooping pig */
#define I_BIRD		'B'	/*    45 */
#define I_SBIRD		'C'	/*     5 */	/* swooping bird */
#define	I_ROCK		'o'	/* 1  45 */
#define	I_SHOOTER	'S'	/*  2  5 */

#define I_BONUSBOX	'X'	/* 12345 */
#define I_PIEBOX	'Y'	/* 12345 */
#define	I_FOODBOX	'W'	/* 12345 */
#define	I_DOLLBOX	'D'	/* 12345 */
#define	I_TELEPORTBOX	'Q'	/* 12345 */
#define	I_REENTRY	'R'	/* 12345 */
#define	I_BALLBOX	'O'	/* 12345 */
#define	I_BANGBOX	'!'	/* 12345 */

/* contents of boxes */

#define I_BAG		'\002'	/* not used on input */
#define I_HONKER	'\003'	/* not used on input */
#define I_PIEITEM	'\004'	/* not used on input */
#define I_BALLITEM	'\005'	/* not used on input */
#define I_SHAKE		'\006'	/* not used on input */
#define I_BURGER	'\007'	/* not used on input */
#define I_HOTDOG	'\010'	/* not used on input */
#define I_MUG		'\011'	/* not used on input */
#define I_DOLLITEM	'\012'	/* not used on input */
#define I_TELEPORTITEM	'\013'	/* not used on input */
#define	I_BANGITEML	'\014'	/* not used on input */
#define	I_BANGITEMR	'\015'	/* not used on input */
#define	MAGICITEMS	"\2\3\4\5\6\7\10\11\12\13\14\15"

#endif
HIPPOPOTAMUS_ON_A_POGO_STICK

chmod 644 codes.h

