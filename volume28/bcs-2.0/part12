# ============= bcs-2.0/doc/tex3patch ==============
if test -f 'bcs-2.0/doc/tex3patch' -a X"$1" != X"-c"; then
	echo 'x - skipping bcs-2.0/doc/tex3patch (File already exists)'
	rm -f _shar_wnt_.tmp
else
> _shar_wnt_.tmp
echo 'x - extracting bcs-2.0/doc/tex3patch (Text)'
sed 's/^X//' << 'SHAR_EOF' > 'bcs-2.0/doc/tex3patch' &&
#!/bin/sh
# Auxiliary script to work around TeX 3.0 bug.      ---- tex3patch  ----
# patches texinfo.tex in current directory, or in directory given as arg.
X
ANYVERSION=no
X
for arg in $1 $2
do
X	case $arg in 
X		--dammit | -d ) ANYVERSION=yes ;;
X
X		* ) dir=$arg
X	esac
done
X
if [ -z "$dir" ]; then
X  dir='.'
fi
X
if [ \( 2 -lt $# \) -o \
X     \( ! -f $dir/texinfo.tex \)   ]; then
X  echo "To patch texinfo.tex for peaceful coexistence with Unix TeX 3.0,"
X  echo "run    $0"
X  echo "with no arguments in the same directory as texinfo.tex; or run"
X  echo "       $0 DIRECTORY"
X  echo "(where DIRECTORY is a path leading to texinfo.tex)."
X  exit
fi
X
if [ -z "$TMPDIR" ]; then
X  TMPDIR=/tmp
fi
X
echo "Checking for \`dummy.tfm'"
X
( cd $TMPDIR; tex '\relax \batchmode \font\foo=dummy \bye' )
X
grep -s '3.0' $TMPDIR/texput.log
if [ 1 = "$?" -a "$ANYVERSION" != "yes" ]; then
X	echo "You probably do not need this patch,"
X        echo "since your TeX does not seem to be version 3.0."
X	echo "If you insist on applying the patch, run $0"
X	echo "again with the option \`--dammit'"
X	exit
fi
X
grep -s 'file not found' $TMPDIR/texput.log
if [ 0 = $? ]; then
X	echo "This patch requires the dummy font metric file \`dummy.tfm',"
X	echo "which does not seem to be part of your TeX installation."
X	echo "Please get your TeX maintainer to install \`dummy.tfm',"
X	echo "then run this script again."
X	exit
fi
rm $TMPDIR/texput.log
X
echo "Patching $dir/texinfo.tex"
X
sed -e 's/%%*\\font\\nullfont/\\font\\nullfont/' \
X    $dir/texinfo.tex >$TMPDIR/texinfo.tex
mv $dir/texinfo.tex $dir/texinfo.tex-distrib; mv $TMPDIR/texinfo.tex $dir
X
if [ 0 = $? ]; then
X	echo "Patched $dir/texinfo.tex to avoid TeX 3.0 bug."
X	echo "The original version is saved as $dir/texinfo.tex-distrib."
else
X	echo "Patch failed.  Sorry."
fi
----------------------------------------tex3patch ends
X
X
SHAR_EOF
chmod 0775 bcs-2.0/doc/tex3patch ||
echo 'restore of bcs-2.0/doc/tex3patch failed'
Wc_c="`wc -c < 'bcs-2.0/doc/tex3patch'`"
test 1845 -eq "$Wc_c" ||
	echo 'bcs-2.0/doc/tex3patch: original size 1845, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
# ============= bcs-2.0/doc/baseline.EPS ==============
if test -f 'bcs-2.0/doc/baseline.EPS' -a X"$1" != X"-c"; then
	echo 'x - skipping bcs-2.0/doc/baseline.EPS (File already exists)'
	rm -f _shar_wnt_.tmp
else
> _shar_wnt_.tmp
echo 'x - extracting bcs-2.0/doc/baseline.EPS (Text)'
sed 's/^X//' << 'SHAR_EOF' > 'bcs-2.0/doc/baseline.EPS' &&
%!PS-Adobe-2.0 EPSF-2.0
%%Title: baseline.fig
%%Creator: fig2dev Version 2.1.8 Patchlevel 1
%%CreationDate: Sat Mar 19 23:36:43 1994
%%For: qjb@tabetha (Jay Berkenbilt)
%%Orientation: Portrait
%%BoundingBox: 0 0 137 301
%%Pages: 0
%%EndComments
/$F2psDict 200 dict def 
$F2psDict begin
$F2psDict /mtrx matrix put
/l {lineto} bind def
/m {moveto} bind def
/s {stroke} bind def
/n {newpath} bind def
/gs {gsave} bind def
/gr {grestore} bind def
/clp {closepath} bind def
/graycol {dup dup currentrgbcolor 4 -2 roll mul 4 -2 roll mul
4 -2 roll mul setrgbcolor} bind def
/col-1 {} def
/col0 {0 0 0 setrgbcolor} bind def
/col1 {0 0 1 setrgbcolor} bind def
/col2 {0 1 0 setrgbcolor} bind def
/col3 {0 1 1 setrgbcolor} bind def
/col4 {1 0 0 setrgbcolor} bind def
/col5 {1 0 1 setrgbcolor} bind def
/col6 {1 1 0 setrgbcolor} bind def
/col7 {1 1 1 setrgbcolor} bind def
/col8 {.68 .85 .9 setrgbcolor} bind def
/col9 {0 .39 0 setrgbcolor} bind def
/col10 {.65 .17 .17 setrgbcolor} bind def
/col11 {1 .51 0 setrgbcolor} bind def
/col12 {.63 .13 .94 setrgbcolor} bind def
/col13 {1 .75 .8 setrgbcolor} bind def
/col14 {.7 .13 .13 setrgbcolor} bind def
/col15 {1 .84 0 setrgbcolor} bind def
X	end
/$F2psBegin {$F2psDict begin /$F2psEnteredState save def} def
/$F2psEnd {$F2psEnteredState restore end} def
%%EndProlog
X
$F2psBegin
0 setlinecap 0 setlinejoin
-49.0 373.0 translate 0.900 -0.900 scale
0.500 setlinewidth
