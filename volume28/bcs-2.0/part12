#!/bin/sh
# this is bcs.12 (part 12 of bcs-2.0)
# do not concatenate these parts, unpack them in order with /bin/sh
# file bcs-2.0/doc/stageb.fig continued
#
if test ! -r _shar_seq_.tmp; then
	echo 'Please unpack part 1 first!'
	exit 1
fi
(read Scheck
 if test "$Scheck" != 12; then
	echo Please unpack part "$Scheck" next!
	exit 1
 else
	exit 0
 fi
) < _shar_seq_.tmp || exit 1
if test ! -f _shar_wnt_.tmp; then
	echo 'x - still skipping bcs-2.0/doc/stageb.fig'
else
echo 'x - continuing file bcs-2.0/doc/stageb.fig'
sed 's/^X//' << 'SHAR_EOF' >> 'bcs-2.0/doc/stageb.fig' &&
2 1 0 1 -1 0 0 0 0.000 -1 1 0
X	0 0 1.000 4.000 8.000
X	 419 159 404 159 9999 9999
2 1 0 1 -1 0 0 0 0.000 7 0 0
X	 144 79 84 139 9999 9999
2 1 0 1 -1 0 0 0 0.000 7 0 0
X	 144 79 124 139 9999 9999
2 1 0 1 -1 0 0 0 0.000 7 0 0
X	 144 79 164 139 9999 9999
2 1 0 1 -1 0 0 0 0.000 7 0 0
X	 144 79 204 139 9999 9999
2 1 0 1 -1 0 0 0 0.000 7 0 0
X	 84 159 64 219 9999 9999
2 1 0 1 -1 0 0 0 0.000 7 0 0
X	 84 164 104 219 9999 9999
2 1 0 1 -1 0 0 0 0.000 7 0 0
X	 104 239 84 299 9999 9999
2 1 0 1 -1 0 0 0 0.000 7 0 0
X	 104 239 124 299 9999 9999
2 1 0 1 -1 0 0 0 0.000 7 0 0
X	 124 319 124 379 9999 9999
4 0 12 12 0 -1 0 0.00000 4 14 7 339 154 A
4 0 12 12 0 -1 0 0.00000 4 14 7 379 154 B
4 0 12 12 0 -1 0 0.00000 4 14 7 419 154 C
4 0 12 12 0 -1 0 0.00000 4 14 7 319 234 E
4 0 12 12 0 -1 0 0.00000 4 14 21 374 314 RCS
4 0 12 12 0 -1 0 0.00000 4 14 21 114 314 RCS
4 0 12 12 0 -1 0 0.00000 4 14 7 79 154 A
4 0 12 12 0 -1 0 0.00000 4 14 7 119 154 B
4 0 12 12 0 -1 0 0.00000 4 14 7 159 154 C
4 0 12 12 0 -1 0 0.00000 4 14 7 59 234 E
4 0 12 12 0 -1 0 0.00000 4 14 7 204 154 D
4 0 12 12 0 -1 0 0.00000 4 14 7 104 234 F
4 0 12 12 0 -1 0 0.00000 4 14 21 74 314 a.c
4 0 12 12 0 -1 0 0.00000 4 14 35 109 394 a.c,v
4 0 12 12 0 -1 0 0.00000 4 14 56 119 74 Baseline
4 0 12 12 0 -1 0 0.00000 4 14 49 379 74 Staging
4 0 12 12 0 -1 0 0.00000 4 14 7 364 234 F
4 0 12 12 0 -1 0 0.00000 4 14 7 464 154 D
4 0 12 12 0 -1 0 0.00000 4 14 21 334 314 a.c
SHAR_EOF
echo 'File bcs-2.0/doc/stageb.fig is complete' &&
chmod 0444 bcs-2.0/doc/stageb.fig ||
echo 'restore of bcs-2.0/doc/stageb.fig failed'
Wc_c="`wc -c < 'bcs-2.0/doc/stageb.fig'`"
test 2227 -eq "$Wc_c" ||
	echo 'bcs-2.0/doc/stageb.fig: original size 2227, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
# ============= bcs-2.0/doc/stageb.txt ==============
if test -f 'bcs-2.0/doc/stageb.txt' -a X"$1" != X"-c"; then
	echo 'x - skipping bcs-2.0/doc/stageb.txt (File already exists)'
	rm -f _shar_wnt_.tmp
else
> _shar_wnt_.tmp
echo 'x - extracting bcs-2.0/doc/stageb.txt (Text)'
sed 's/^X//' << 'SHAR_EOF' > 'bcs-2.0/doc/stageb.txt' &&
@c character below required to first line from being misindented.
-
X
X              Baseline                           Staging
X                 |                                  |
X        +-----+--+--+-----+                +-----+--+--+-----+
X        |     |     |     |                |     |     |     |
X        |     |     |     |                |     |     |     |
X        |     |     |     |                |     |     |     |
X        A     B     C     D             <--A  <--B  <--C  <--D
X       /\                                 /\
X      /  \                               /  \
X     /    \                             /    \
X    /      \                           /      \
X    E      F                        <--E   <--F
X          /\                                 /\
X         /  \                               /  \
X        /    \                             /    \
X       /      \                           /      \
X      a.c     RCS                        a.c  <--RCS
X               |
X               |
X               |
X               |
X             a.c,v
SHAR_EOF
chmod 0444 bcs-2.0/doc/stageb.txt ||
echo 'restore of bcs-2.0/doc/stageb.txt failed'
Wc_c="`wc -c < 'bcs-2.0/doc/stageb.txt'`"
test 1067 -eq "$Wc_c" ||
	echo 'bcs-2.0/doc/stageb.txt: original size 1067, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
# ============= bcs-2.0/doc/stagec.fig ==============
