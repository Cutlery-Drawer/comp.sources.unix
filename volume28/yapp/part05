if test -f 'item.c' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'item.c'\"
else
echo shar: Extracting \"'item.c'\" \(32474 characters\)
sed "s/^X//" >'item.c' <<'END_OF_FILE'
X/* ITEM.C */
Xstatic	char sccsid[] = "@(#)item.c 1.3 94/01/20 (c)1993 thalerd";
X#include <stdio.h>
X#include <time.h>
X#include <string.h>
X#include <sys/types.h> /* for sys/stat.h on ultrix */
X#include <sys/stat.h> /* for stat() */
X#include <stdlib.h> /* for getenv() */
X#include <ctype.h>  /* for tolower() */
X#include <errno.h>
X#include <memory.h> /* for memcpy() */
X#include "config.h"
X#include "struct.h"
X#include "macro.h"
X#include "item.h"
X#include "range.h"
X#include "globals.h"
X#include "lib.h"
X#include "arch.h"
X#include "system.h"
X#include "sum.h"
X#include "sep.h"
X#include "conf.h"   /* for checkpoint() */
X#include "xalloc.h"
X#include "edbuf.h"
X#include "main.h"
X#include "driver.h"
X#include "files.h"
X#include "news.h"
X#include "stats.h" /* for get_config */
X
XFILE *ext_fp;
Xshort ext_first,ext_last; /* for 'again' */
X
X/******************************************************************************/
X/* CREATE A NEW ITEM                                                          */
X/******************************************************************************/
Xint
Xdo_enter(this,sub,text,idx,sum,part,stt,art,mid,uid,login,fullname)
Xsumentry_t  *this; /*   New item summary */
Xchar        *sub;  /*   New item subject */
Xchar       **text; /*   New item text    */
Xshort        idx;
Xsumentry_t  *sum;
Xpartentry_t *part;
Xstatus_t    *stt;
Xlong         art;
Xchar        *mid;
Xuid_t        uid;
Xchar        *login;
Xchar        *fullname;
X{
X   short item,line;
X   FILE *fp;
X   char buff[MAX_LINE_LENGTH];
X   long mod;
X
X   item = ++(stt->i_last); /* next item number */
X   if (!art) printf("Saving as item %d...",item);
X   sprintf(buff,"%s/_%d",conflist[idx].location,item);
X   mod = O_W|O_EXCL;
X   if (stt->c_security & CT_BASIC) mod |= O_PRIVATE;
X   if ((fp=mopen(buff,mod))==NULL) return 0;
X   fprintf(fp,"!<ps02>\n,H%s\n,R%04X\n,U%d,%s\n,A%s\n,D%lX\n,T\n",
X      sub,RF_NORMAL,uid,login,fullname,this->first);
X
X   if (art) {
X      fprintf(fp,",N%06ld\n",art);
X      fprintf(fp,",M%s\n",mid);
X   } else {
X      for (line=0; line<xsizeof(text); line++) {
X         if (text[line][0]==',') fprintf(fp,",,%s\n",text[line]);
X         else                    fprintf(fp,"%s\n",text[line]);
X      }
X   }
X   fprintf(fp,",E\n");
X
X   /* If sum doesn't exist, we must make sure the data has
X    * been written before the sum file is created.
X    */
X   fflush(fp);
X   memcpy(&sum[stt->i_last-1],this,sizeof(sumentry_t));
X   save_sum(sum,(short)(stt->i_last-1),idx,stt);
X   mclose(fp);
X   if (!art) wputs("saved.\n");
X   stt->i_numitems++;
X   return 1;
X}
X
X/******************************************************************************/
X/* ENTER A NEW ITEM                                                           */
X/******************************************************************************/
Xint              /* RETURNS: (nothing)     */
Xenter(argc,argv) /* ARGUMENTS:             */
Xint    argc;     /*    Number of arguments */
Xchar **argv;     /*    Argument list       */
X{
X   char buff[MAX_LINE_LENGTH],sub[MAX_LINE_LENGTH],**file;
X   char cfbufr[MAX_LINE_LENGTH];
X   sumentry_t this;
X
X   if (st_glob.c_status & CS_NORESPONSE) {
X      printf("You only have readonly access.\n");
X      return 1;
X   }
X
X   if (argc>1) {
X      printf("Bad item range near \"%s\"\n",argv[1]);
X      return 1;
X   }
X
X   refresh_sum(0,confidx,sum,part,&st_glob);
X   if (!st_glob.i_numitems && !(st_glob.c_status & CS_FW)) {
X      wputs("Only the fair-witness can enter the first item.  Sorry!\n");
X      return 1;
X   }
X   if (st_glob.i_last == MAX_ITEMS) { /* numbered 1-last */
X      wputs("UNK Too many items.\n");
X      return 1;
X   }
X
X#ifdef NEWS
X   if (st_glob.c_security & CT_NEWS) {
X      char rnh[MAX_LINE_LENGTH];
X
X      make_rnhead(re,0);
X      sprintf(rnh,"%s/.rnhead",home);
X      sprintf(buff,"Pnews -h %s",rnh);
X      unix_cmd(buff);
X      rm(rnh,SL_USER);
X      return 1;
X   } 
X#endif
X
X   sprintf(cfbufr,"%s/cf.buffer",work);
X   if (text_loop(1,0)) {
X      if (!(file = grab_file(work,"cf.buffer",0))) 
X         wputs("can't open cfbuffer\n");
X      else {
X         /* Get the subject */
X         do {
X            if (status & S_MOTIF)
X               wgets(sub,"Enter a one line header or ':' to edit");
X            else {
X               printf("Enter a one line header or ':' to edit\n? ");
X               ngets(sub, st_glob.inp);
X            }
X            if (!strcmp(sub,":")) {
X               xfree(file);
X
X               if (!edit(cfbufr,NULL,0) 
X                || !(file = grab_file(work,"cf.buffer",0))) {
X                  wputs("can't open cfbuffer\n");
X                  return 1;
X               }
X               sub[0]=0;
X               continue;
X            }
X            if (!strlen(sub) && get_yes("Ok to abandon item entry? ")) {
X               rm(cfbufr,SL_USER);
X               wputs("No text -- item entry aborted!\n");
X               xfree(file);
X               return 1;
X            }
X         } while (!strlen(sub));
X
X         /* Verify item entry */
X         if (!get_yes("Ok to enter this item? ")) {
X            rm(cfbufr,SL_USER);
X            wputs("No text -- item entry aborted!\n");
X            xfree(file);
X            return 1;
X         }
X
X         this.nr = 1;
X         this.flags = IF_ACTIVE;
X         this.last  = time(&( this.first ));
X   
X         /* make sure nothing changed since we started */
X         refresh_sum(0,confidx,sum,part,&st_glob);
X         /* dont need to free anything */
X
X         do_enter(&this,sub,file,confidx,sum,part,&st_glob,0,NULL,
X	  uid,login,st_glob.fullname);
X         xfree(file);
X      }
X   }
X   rm(cfbufr,SL_USER);
X   return 1;
X}
X
X/******************************************************************************/
X/* DISPLAY CURRENT ITEM HEADER                                                */
X/* Only requires st_glob.i_current to be set, nothing else                    */
X/******************************************************************************/
Xvoid
Xshow_header()
X{
X   short tmp,pr;
X   FILE *fp;
X   char  buff[MAX_LINE_LENGTH];
X
X   open_pipe();
X   sprintf(buff,"%s/_%d",conflist[confidx].location,st_glob.i_current);
X   if (fp=mopen(buff,O_R)) {
X      st_glob.r_current = 0; /* current resp = header */
X      get_resp(fp,re,(short)GR_HEADER,(short)0); /* Get header of #0 */
X/* The problem here is that itemsep uses r_current as an index to
X   the information in re, so we can't show # new responses
X      st_glob.r_current = sum[st_glob.i_current-1].nr
X       - abs(part[st_glob.i_current-1].nr);
X */
X
X      /* Get info about the actual item text if possible */
X      if (re[st_glob.r_current].flags & (RF_EXPIRED|RF_SCRIBBLED)) 
X	 pr = 0;
X      else if (re[st_glob.r_current].flags & RF_CENSORED) 
X         pr = ((st_glob.opt_flags & OF_NOFORGET) || !(flags & O_FORGET));
X      else 
X	 pr = 1;
X
X      if (pr) get_resp(fp,&(re[st_glob.r_current]),(short)GR_ALL,st_glob.r_current);
X      if ((re[st_glob.r_current].flags & RF_SCRIBBLED)
X       && re[st_glob.r_current].numchars>7) {
X         fseek(fp,re[st_glob.r_current].textoff,0);
X         ngets(buff,fp);
X         re[st_glob.r_current].text = (char**)buff;
X      }
X
X      sepinit(IS_CENSORED|IS_UID|IS_DATE);
X      itemsep((st_glob.opt_flags & OF_SHORT)?"ishort":"isep",0);
X      if (pr) xfree(re[st_glob.r_current].text);
X      st_glob.r_current = 0; /* current resp = header */
X      mclose(fp);
X   }
X}
X
Xvoid
Xshow_nsep(fp)
XFILE *fp;
X{
X   short tmp,i=st_glob.r_first;
X   tmp = st_glob.r_current;
X
X   if (st_glob.since) {
X      while (i<sum[st_glob.i_current-1].nr && st_glob.since > re[i].date) {
X         i++;
X         get_resp(fp,&(re[i]),(short)GR_HEADER,i);
X         if (i>=sum[st_glob.i_current-1].nr) break;
X      }
X   } else if (!st_glob.r_first)
X      i = abs(part[st_glob.i_current-1].nr);
X   if (!i) i++;
X
X   st_glob.r_current = sum[st_glob.i_current-1].nr-i; /* calc # new */
X   itemsep("nsep",0);
X   st_glob.r_current = tmp;
X}
X
X/******************************************************************************/
X/* SHOW CURRENT RESPONSE                                                      */
X/******************************************************************************/
Xvoid
Xshow_resp(fp)
XFILE *fp;
X{
X   short pr;
X   char buff[MAX_LINE_LENGTH];
X
X   get_resp(fp,&(re[st_glob.r_current]),(short)GR_HEADER,st_glob.r_current);
X      
X   /* Print response text */
X   if (re[st_glob.r_current].flags & (RF_EXPIRED|RF_SCRIBBLED)) 
X      pr = 0;
X   else if (re[st_glob.r_current].flags & RF_CENSORED) 
X      pr = ((st_glob.opt_flags & OF_NOFORGET) || !(flags & O_FORGET));
X   else 
X      pr = 1;
X
X   /* Get resp to initialize stats like %s itemsep */
X   if (pr)
X      get_resp(fp,&(re[st_glob.r_current]),(short)GR_ALL,st_glob.r_current);
X   if ((re[st_glob.r_current].flags & RF_SCRIBBLED )
X    && re[st_glob.r_current].numchars>7) {
X      fseek(fp,re[st_glob.r_current].textoff,0);
X      ngets(buff,fp);
X      re[st_glob.r_current].text = (char**)buff;
X   }
X
X   /* Print response header */
X   if (st_glob.r_current) {
X      sepinit(IS_PARENT|IS_CENSORED|IS_UID|IS_DATE);
X      itemsep("rsep",0);
X   }
X
X   /* read short could do response headers only to browse resps */
X   /* if (pr && !(st_glob.opt_flags & OF_SHORT)) */
X   if (pr) {
X      if (!st_glob.r_current) wfputc('\n',st_glob.outp);
X      for (st_glob.l_current=0;
X           st_glob.l_current<xsizeof(re[st_glob.r_current].text)
X        && !(status & S_INT); 
X           st_glob.l_current++) {
X         itemsep("txtsep",0);
X      }
X      xfree(re[st_glob.r_current].text);
X   }
X      
X   /* Count it as seen */
X   if (st_glob.r_lastseen<=st_glob.r_current)
X      st_glob.r_lastseen=st_glob.r_current+1;
X}
X
X/******************************************************************************/
X/* SHOW RANGE OF RESPONSES                                                    */
X/******************************************************************************/
Xvoid
Xshow_range()
X{
X   FILE *fp;
X   char  buff[MAX_LINE_LENGTH];
X
X   if (st_glob.r_first > st_glob.r_last) return; /* invalid range */
X   refresh_sum(st_glob.i_current,confidx,sum,part,&st_glob);
X   /* in case new reponses came */
X   st_glob.r_max=sum[ st_glob.i_current-1 ].nr-1; 
X   st_glob.r_current = sum[st_glob.i_current-1].nr
X    - abs(part[st_glob.i_current-1].nr);
X
X   open_pipe();
X   sprintf(buff,"%s/_%d",conflist[confidx].location,st_glob.i_current);
X   if (fp=mopen(buff,O_R)) {
X
X/*    if (!st_glob.r_current && sum[st_glob.i_current-1].nr>1) */
X/*    if ( st_glob.r_first   && sum[st_glob.i_current-1].nr>1)    */
X
X      /* For each response */
X      for (st_glob.r_current = st_glob.r_first;
X           st_glob.r_current<= st_glob.r_last 
X            && st_glob.r_current<=st_glob.r_max
X            && !(status & S_INT);
X           st_glob.r_current++) {
X         if (st_glob.r_first==0 && st_glob.r_current==1)
X            show_nsep(fp); /* nsep between resp 0 and 1 */
X         show_resp(fp);
X      }
X      status &= ~S_INT; /* Int terminates range */
X
X      mclose(fp);
X      st_glob.r_current--;
X      itemsep("zsep",0);
X   }
X}
X
X/******************************************************************************/
X/* READ THE CURRENT ITEM                                                      */
X/******************************************************************************/
Xvoid
Xread_item()
X{
X   FILE *fp;
X   short i_lastseen;    /* Macro for current item index */
X   char  buff[MAX_LINE_LENGTH];
X   short oldnr,   topt_flags;
X   long  oldlast;
X   time_t tsince;
X
X   sepinit(IS_ITEM|IS_ALL);
X   i_lastseen         = st_glob.i_current-1;
X   oldnr = st_glob.r_lastseen = abs(part[i_lastseen].nr);
X   st_glob.r_current=0;
X   oldlast = part[i_lastseen].last;
X
X   /* Open file */
X   sprintf(buff,"%s/_%d",conflist[confidx].location,st_glob.i_current);
X   if (!(fp=mopen(buff,O_R))) return;
X   if (!ngets(buff,fp) || strcmp(buff,"!<ps02>")) {
X      wputs("UNK Invalid _N file format\n");
X      mclose(fp);
X      return;
X   }
X   if (!ngets(buff,fp) || strncmp(buff,",H",2)) {
X      wputs("UNK Invalid _N file format\n");
X      mclose(fp);
X      return;
X   }
X
X   /* Get all the response offsets       */
X   /* censor/scribble require this setup */
X   get_item(fp,st_glob.i_current,re,sum); 
X   /* moved up here from above response set loop */
X
X   get_resp(fp,re,(short)GR_HEADER,(short)0); /* Get header of #0 */
X   if (st_glob.opt_flags & (OF_NEWRESP|OF_NORESPONSE))
X      st_glob.r_first = abs(part[i_lastseen].nr);
X   else if (st_glob.since) {
X      st_glob.r_first=0;
X      while (st_glob.since > re[st_glob.r_first].date) {
X         st_glob.r_first++;
X         get_resp(fp,&(re[st_glob.r_first]),(short)GR_HEADER,st_glob.r_first);
X         if (st_glob.r_first>=sum[i_lastseen].nr) break;
X      }
X   } 
X/* else st_glob.r_first=0; LLL */
X
X   /* Display item header */
X   open_pipe();
X   show_header();
X 
X   st_glob.r_last = MAX_RESPONSES;
X   st_glob.r_max=sum[i_lastseen].nr-1;
X
X   /* For each response set */
X   if (!(st_glob.opt_flags & OF_NORESPONSE)
X       && st_glob.r_first<=st_glob.r_max) {
X      if (st_glob.r_first>0)
X         show_nsep(fp); /* nsep between header and responses */
X      show_range();
X   } else
X      st_glob.r_current = st_glob.r_first; /* for -# command */
X   if (!(st_glob.opt_flags & OF_PASS))
X      status &= ~S_INT;
X
X   /* Save range info */
X   topt_flags = st_glob.opt_flags;
X   tsince     = st_glob.since;
X   strcpy(buff, st_glob.string);
X   ext_fp     = fp;
X   ext_first  = st_glob.r_first;
X   ext_last   = st_glob.r_last;
X
X   /* RFP loop until pass */
X   /* while (!(status & S_INT) && (st_glob.r_last >= 0))*/
X   while (st_glob.r_last >= 0) 
X   {
X
X      /* Temporarily mark seen, for forget command */
X      part[i_lastseen].nr   = st_glob.r_lastseen;
X      if ((sum[i_lastseen].flags & IF_FORGOTTEN) && part[i_lastseen].nr>0)
X         part[i_lastseen].nr = -part[i_lastseen].nr; /* stay forgotten */
X/* Don't do this, acc to Russ
X      time(&(part[i_lastseen].last));
X*/
X   
X      /* Main RFP cmd loop */
X      mode = M_RFP;
X      if (status & S_MOTIF)
X         command("pass",0);
X      else {
X			short tmp_stat;
X
X			tmp_stat = st_glob.c_status;
X         if ((sum[i_lastseen].flags & IF_FROZEN) || (flags & O_READONLY))
X            st_glob.c_status |= CS_NORESPONSE;
X         /* while (mode==M_RFP && !(status & S_INT)) */
X         /* test stop in case ^D hit */
X         while (mode==M_RFP && !(status & S_STOP)) {
X            if (st_glob.opt_flags & OF_PASS)
X               command("pass",0);
X            else if (!get_command("pass")) { /* eof */
X               /* status |= S_INT; WHY was this here? */
X               if (!(status & S_STOP)) {
X                  status |= S_STOP; /* instead */
X                  wputs("Stopping.\n");
X               }
X
X               mode = M_OK;
X               st_glob.r_last = -1;
X
X               if (st_glob.opt_flags & OF_REVERSE)
X                  st_glob.i_current = st_glob.i_first;
X               else
X                 st_glob.i_current = st_glob.i_last;
X            }
X         }
X         st_glob.c_status = tmp_stat;
X      }
X   }
X/* KKK what if new items come in? */
X
X   /* Save range info */
X   st_glob.opt_flags = topt_flags;
X   st_glob.since = tsince;
X   strcpy(st_glob.string, buff);
X
X   /* Save to lastseen */
X   if (st_glob.r_last > -2) { /* unless preserved or forgotten */
X      part[i_lastseen].nr   = st_glob.r_lastseen;
X      if ((sum[i_lastseen].flags & IF_FORGOTTEN) && part[i_lastseen].nr>0)
X         part[i_lastseen].nr = -part[i_lastseen].nr; /* stay forgotten */
X      if (abs(part[i_lastseen].nr)==sum[i_lastseen].nr)
X         time(&(part[i_lastseen].last));
X      if (flags & O_AUTOSAVE) {
X			char **config;
X
X			if (config = get_config(confidx))
X			   write_part(config[CF_PARTFILE]);
X      }
X   } else if (st_glob.r_last == -2) { /* unless preserved or forgotten */
X      part[i_lastseen].nr   = oldnr;
X      part[i_lastseen].last = oldlast;
X   }
X
X   /* Must be kept open past RFP mode so 'since' command can access it */
X   mclose(fp);
X}
X
X/******************************************************************************/
X/* READ A SET OF ITEMS IN THE CONFERENCE                                      */
X/******************************************************************************/
Xint                /* RETURNS: (nothing)     */
Xdo_read(argc,argv) /* ARGUMENTS:             */
Xint    argc;       /*    Number of arguments */
Xchar **argv;       /*    Argument list       */
X{
X   short      i_s,i_i, shown=0, rfirst=0,br;
X   char       act[MAX_ITEMS];
X
X   br = (match(argv[0],"b_rowse") || match(argv[0],"s_can"));
X   rangeinit(&st_glob,act);
X   refresh_sum(0,confidx,sum,part,&st_glob);
X   st_glob.r_first = 0;
X   st_glob.opt_flags = (br)? OF_SHORT|OF_NORESPONSE|OF_PASS : 0; 
X
X   /* Check arguments */
X   if (argc>1)
X      range(argc,argv,&st_glob.opt_flags,act,sum,&st_glob,0);
X
X   if (!(st_glob.opt_flags & OF_RANGE)) {
X      rangetoken("all",&st_glob.opt_flags,act,sum,&st_glob);
X      if (!(st_glob.opt_flags & (OF_UNSEEN|OF_FORGOTTEN|OF_RETIRED|OF_NEWRESP|OF_BRANDNEW))
X       && !st_glob.since && !br) {
X         rangetoken("new",&st_glob.opt_flags,act,sum,&st_glob);
X      }
X   }
X   if (st_glob.opt_flags & OF_REVERSE) {
X      i_s = st_glob.i_last;
X      i_i = -1;
X   } else {
X      i_s = st_glob.i_first;
X      i_i = 1;
X   }
X   rfirst = st_glob.r_first;
X
X   if (match(argv[0],"pr_int"))
X      confsep("printmsg",confidx,&st_glob,part,0);
X
X   /* Process items */
X   sepinit(IS_START); 
X   for (st_glob.i_current = i_s; 
X            st_glob.i_current >= st_glob.i_first 
X         && st_glob.i_current <= st_glob.i_last 
X         && !(status & S_INT); 
X        st_glob.i_current += i_i) {
X      if (cover(st_glob.i_current,confidx,st_glob.opt_flags,act[st_glob.i_current-1],sum,
X       part,&st_glob)) {
X         st_glob.r_first = rfirst;
X         read_item();
X         shown++;
X         if (match(argv[0],"pr_int"))
X            wputs("\n");
X      }
X   }
X   if (!shown && (st_glob.opt_flags & (OF_BRANDNEW|OF_NEWRESP)))
X      wputs("No new items matched.\n");
X   
X   /* Check for new mail only */
X   refresh_stats(sum,part,&st_glob);
X   check_mail(0);
X
X   return 1;
X}
X
X/******************************************************************************/
X/* FORGET A SET OF ITEMS IN THE CONFERENCE                                    */
X/******************************************************************************/
Xint               /* RETURNS: (nothing)     */
Xforget(argc,argv) /* ARGUMENTS:             */
Xint    argc;      /*    Number of arguments */
Xchar **argv;      /*    Argument list       */
X{
X   char       act[MAX_ITEMS];
X   short      j;
X
X   rangeinit(&st_glob,act);
X   refresh_sum(0,confidx,sum,part,&st_glob);
X
X   if (argc<2) {
X      rangetoken("all",&st_glob.opt_flags,act,sum,&st_glob);
X   } else { /* Process args */
X      range(argc,argv,&st_glob.opt_flags,act,sum,&st_glob,0);
X   }
X
X   /* Process items */
X   for (j=st_glob.i_first; j<=st_glob.i_last && !(status & S_INT); j++) {
X      if (cover(j,confidx,st_glob.opt_flags,act[j-1],sum,part,&st_glob)) {
X         printf("Forgetting item %d\n",j);
X         if (!part[j-1].nr) {
X            printf("You must read item %d before you can forget it\n",j);
X         } else {
X            st_glob.r_lastseen = part[j-1].nr    = -part[j-1].nr;
X            sum[j-1].flags |= IF_FORGOTTEN;
X            time(&( part[j-1].last )); /* current time */
X         }
X      }
X   }
X   st_glob.r_last = -1; /* go on to next item if at RFP prompt */
X   return 1;
X}
X
X/******************************************************************************/
X/* LOCATE A WORD OR GROUP OF WORDS IN A CONFERENCE                            */
X/******************************************************************************/
Xint                /* RETURNS: (nothing)     */
Xdo_find(argc,argv) /* ARGUMENTS:             */
Xint    argc;       /*    Number of arguments */
Xchar **argv;       /*    Argument list       */
X{
X   char       act[MAX_ITEMS],buff[MAX_LINE_LENGTH],pr,
X              astr[MAX_LINE_LENGTH],
X              str[MAX_LINE_LENGTH];
X   short      j,icur;
X   FILE      *fp;
X
X   icur = st_glob.i_current;
X   rangeinit(&st_glob,act);
X   refresh_sum(0,confidx,sum,part,&st_glob);
X
X   if (argc>1) /* Process argc */
X      range(argc,argv,&st_glob.opt_flags,act,sum,&st_glob,0);
X   if (!(st_glob.opt_flags & OF_RANGE))
X      rangetoken("all",&st_glob.opt_flags,act,sum,&st_glob);
X
X   if (!st_glob.string[0] && !st_glob.author[0]) {
X      wputs("Find: look for what?\n");
X      return 1;
X   }
X   strcpy(str, lower_case(st_glob.string)); /* so items match */
X   strcpy(astr,lower_case(st_glob.author)); /* so items match */
X   st_glob.string[0]='\0';
X   st_glob.author[0]='\0';
X
X   /* Process items */
X   open_pipe();
X   sepinit(IS_START);
X   for (j=st_glob.i_first; j<=st_glob.i_last && !(status & S_INT); j++) {
X      st_glob.i_current=j;
X      if (cover(j, confidx,st_glob.opt_flags, act[j-1], sum, part,
X       &st_glob)) {
X         sprintf(buff,"%s/_%d",conflist[confidx].location,j);
X         if (!(fp=mopen(buff,O_R))) continue;
X         get_item(fp,j,re,sum);
X         sepinit(IS_ITEM|IS_ALL);
X
X         /* For each response */
X         for (st_glob.r_current = 0;
X              st_glob.r_current < sum[j-1].nr
X               && !(status & S_INT);
X              st_glob.r_current++) {
X      
X            get_resp(fp,&(re[st_glob.r_current]),(short)GR_HEADER,st_glob.r_current);
X      
X            /* Scan response text */
X            if (re[st_glob.r_current].flags & (RF_EXPIRED|RF_SCRIBBLED)) 
X               pr = 0;
X            else if (re[st_glob.r_current].flags & RF_CENSORED) 
X               pr = ((st_glob.opt_flags & OF_NOFORGET) || !(flags & O_FORGET));
X            else 
X               pr = 1;
X
X            if (pr) {
X               get_resp(fp,&(re[st_glob.r_current]),(short)GR_ALL,st_glob.r_current);
X               sepinit(IS_RESP);
X
X               if (!astr[0] 
X                || !strcmp(re[st_glob.r_current].login,astr)) {
X
X                  if (str[0]) {
X                     for (st_glob.l_current=0; 
X                      !(status & S_INT) 
X                       && st_glob.l_current<xsizeof(re[st_glob.r_current].text);
X                      st_glob.l_current++) {
X                        if (strstr(lower_case(
X                         re[st_glob.r_current].text[st_glob.l_current]), str))
X                           itemsep("fsep",0);
X                     }
X                  } else {
X                     st_glob.l_current=0; 
X                     itemsep("fsep",0);
X                  }
X               }
X               xfree(re[st_glob.r_current].text);
X            }
X         }
X         mclose(fp);
X      }
X   }
X
X   /* Reload initial re[] contents */
X   st_glob.i_current = icur;
X   if (icur>=st_glob.i_first && icur<=st_glob.i_last) {
X      sprintf(buff,"%s/_%d",conflist[confidx].location,icur);
X      if (fp=mopen(buff,O_R)) {
X         get_item(fp,j,re,sum);
X         mclose(fp);
X      }
X   }
X   return 1;
X}
X
X/******************************************************************************/
X/* FREEZE/THAW/RETIRE/UNRETIRE A SET OF ITEMS IN THE CONFERENCE               */
X/******************************************************************************/
Xint               /* RETURNS: (nothing)     */
Xfreeze(argc,argv) /* ARGUMENTS:             */
Xint    argc;      /*    Number of arguments */
Xchar **argv;      /*    Argument list       */
X{
X   char       act[MAX_ITEMS],buff[MAX_LINE_LENGTH];
X   short      j;
X   FILE      *fp;
X   struct stat stt;
X
X   rangeinit(&st_glob,act);
X
X   if (argc<2) {
X      printf("Error, no item specified! (try HELP RANGE)\n");
X   } else { /* Process args */
X      range(argc,argv,&st_glob.opt_flags,act,sum,&st_glob,0);
X   }
X
X   /* Process items */
X   for (j=st_glob.i_first; j<=st_glob.i_last && !(status & S_INT); j++) {
X      if (!act[j-1] || !sum[j-1].flags) continue;
X      sprintf(buff,"%s/_%d",conflist[confidx].location,j);
X
X      /* Check for permission */
X      if (!(st_glob.c_status & CS_FW)) {
X         if (!(fp=mopen(buff,O_R))) continue;
X         get_item(fp,j,re,sum);
X         mclose(fp);
X         if (re[0].uid!=getuid()) {
X            printf("You can't do that!\n");
X            continue;
X         }
X      }
X
X#ifndef FREEZE_LINKED
X      if ((sum[j-1].flags & IF_LINKED) && (re[0].uid!=getuid())) {
X         printf("Item %d is linked!\n",j);
X         continue;
X      }
X#endif
X
X      /* Do the change */
X      if (stat(buff,&stt)) {
X         printf("Error accessing %s\n",buff);
X         continue;
X      }
X
X      switch(tolower(argv[0][0])) {
X      case 'f': /* Freeze   r--r--r-- */
X                sum[j-1].flags |=  IF_FROZEN;
X                if (chmod(buff,stt.st_mode & ~S_IWUSR)) error("freezing ",buff);
X					 else
X						 sprintf(buff,"froze item %d", j);
X                break;
X      case 't': /* Thaw     rw-r--r-- */
X                sum[j-1].flags &= ~IF_FROZEN;
X                if (chmod(buff,stt.st_mode |  S_IWUSR)) error("thawing ",buff);
X					 else
X						 sprintf(buff,"thawed item %d", j);
X                break;
X      case 'r': /* Retire   r-xr--r-- F,R */
X                sum[j-1].flags |=  IF_RETIRED;
X                if (chmod(buff,stt.st_mode |  S_IXUSR)) error("retiring ",buff);
X					 else
X						 sprintf(buff,"retired item %d", j);
X                break;
X      case 'u': /* Unretire rw-r--r-- */
X                sum[j-1].flags &= ~IF_RETIRED;
X                if (chmod(buff,stt.st_mode & ~S_IXUSR)) error("unretiring ",buff);
X					 else
X						 sprintf(buff,"unretired item %d", j);
X                break;
X      }
X      save_sum(sum,(short)(j-1),confidx,&st_glob);
X		log(confidx, buff);
X   }
X   return 1;
X}
X
X/******************************************************************************/
X/* LINK AN ITEM INTO THE CONFERENCE                                           */
X/******************************************************************************/
Xint                 /* RETURNS: (nothing)     */
Xlinkfrom(argc,argv) /* ARGUMENTS:             */
Xint    argc;        /*    Number of arguments */
Xchar **argv;        /*    Argument list       */
X{
X   short       idx,i,j;
X   char        sec,
X				 **config;
X   sumentry_t  fr_sum[MAX_ITEMS];
X   char        act[MAX_ITEMS];
X   char        fr_buff[MAX_LINE_LENGTH],
X					to_buff[MAX_LINE_LENGTH],
X					buff[MAX_LINE_LENGTH];
X   status_t    fr_st;
X   partentry_t part2[MAX_ITEMS];
X
X   st_glob.opt_flags = 0;
X   refresh_sum(0,confidx,sum,part,&st_glob);
X   if (!(st_glob.c_status & CS_FW)) {
X      printf("Sorry, you can't do that!\n");
X      return 1;
X   }
X
X   if (argc<2) {
X      printf("No conference specified!\n");
X      return 1;
X   }
X
X   idx = get_idx(argv[1],conflist,maxconf);
X   if (idx<0) {
X      printf("Cannot access conference %s.\n",argv[1]);
X      return 1;
X   }
X
X   /* Read in config file */
X   if (!(config=get_config(idx)))
X      return 1;
X
X   /* Pass security checks (from join() in conf.c) */
X   if (xsizeof(config)>CF_SECURITY) {
X      sec=atoi(config[CF_SECURITY]);
X#ifdef NEWS
X      if (xsizeof(config)<=CF_NEWSGROUP)
X	      sec &= ~CT_NEWS;
X#endif
X   } else
X      sec=0;
X   if (!checkpoint(idx,sec)) { /* Can't linkfrom a READONLY cf */
X      printf("Failed security check on type %d: link aborted.\n",sec);
X      return 1;
X   }
X 
X   /* Get item range */
X   if (argc<3) {
X      printf("Error, no item specified! (try HELP RANGE)\n");
X      return 1;
X   }
X
X#ifdef NEWS
X   fr_st.c_article = 0;
X#endif
X   read_part(config[CF_PARTFILE],part2,&fr_st,idx); /* Read in partfile */
X   fr_st.c_security=(xsizeof(config)>CF_SECURITY)? 
X    atoi(config[CF_SECURITY]) : 0;
X   rangeinit(&fr_st,act);
X
X   /* get subjs for range */
X   get_status(&fr_st,fr_sum,part2,idx);
X
X   range(argc,argv,&st_glob.opt_flags,act,fr_sum,&fr_st,1);
X
X   /* Process items */
X   for (j=fr_st.i_first; j<=fr_st.i_last && !(status & S_INT); j++) {
X      if (!cover(j,idx,st_glob.opt_flags,act[j-1],fr_sum,part2,&fr_st))
X         continue;
X      
X      sprintf(fr_buff,"%s/_%d",conflist[idx].location,j);
X      i = ++(st_glob.i_last); /* next item number */
X      printf("Linking as item %d...",i);
X      sprintf(to_buff,"%s/_%d",conflist[confidx].location,i);
X      if (link(fr_buff,to_buff) && symlink(fr_buff,to_buff))
X         error("linking from ",fr_buff);
X      else {
X         fr_sum[j-1].flags |= IF_LINKED;
X         memcpy(&(sum[i-1]),&(fr_sum[j-1]),sizeof(sumentry_t));
X         /* to_sum[i-1].flags |= IF_LINKED; */
X         save_sum(fr_sum,(short)(j-1),idx,&fr_st);
X         save_sum(sum,(short)(i-1),confidx,&st_glob);
X
X			/* Log action to both conferences */
X			sprintf(buff, "linked item %d to %s %d", j,
X				compress(conflist[confidx].name), i);
X         log(idx, buff);
X			sprintf(buff, "linked item %d from %s %d", i,
X				compress(conflist[idx].name), j);
X         log(confidx, buff);
X
X         printf("done.\n");
X      }
X   }
X   return 1;
X}
X
X/******************************************************************************/
X/* KILL A SET OF ITEMS IN THE CONFERENCE                                      */
X/******************************************************************************/
Xint                /* RETURNS: (nothing)     */
Xdo_kill(argc,argv) /* ARGUMENTS:             */
Xint    argc;       /*    Number of arguments */
Xchar **argv;       /*    Argument list       */
X{
X   char       act[MAX_ITEMS],buff[MAX_LINE_LENGTH],
X              buff2[MAX_LINE_LENGTH];
X   short      j;
X   FILE      *fp;
X
X   rangeinit(&st_glob,act);
X   refresh_sum(0,confidx,sum,part,&st_glob);
X
X   if (argc<2) {
X      printf("Error, no item specified! (try HELP RANGE)\n");
X   } else { /* Process args */
X      range(argc,argv,&st_glob.opt_flags,act,sum,&st_glob,0);
X   }
X
X   /* Process items */
X   for (j=st_glob.i_first; j<=st_glob.i_last && !(status & S_INT); j++) {
X      if (!act[j-1] || !sum[j-1].flags) continue;
X      sprintf(buff,"%s/_%d",conflist[confidx].location,j);
X
X      /* Check for permission */
X      if (!(st_glob.c_status & CS_FW)) {
X         if (!(fp=mopen(buff,O_R))) continue;
X         get_item(fp,j,re,sum);
X         mclose(fp);
X         if (re[0].uid!=getuid() || sum[j-1].nr>1) {
X            wputs("You can't do that!\n");
X            continue;
X         }
X      }
X
X      /* Do the remove */
X      sprintf(buff2,"Ok to kill item %d? ",j);
X      if (get_yes(buff2)) {
X         printf("(Loosing item %d)\n",j);
X         rm(buff,SL_OWNER);
X         sum[j-1].flags=0;
X         save_sum(sum,(short)(j-1),confidx,&st_glob);
X			sprintf(buff2, "killed item %d", j);
X			log(confidx, buff2);
X      }
X   }
X   st_glob.r_last = -1; /* go on to next item if at RFP prompt */
X   return 1;
X}
X
X/******************************************************************************/
X/* REMEMBER (UNFORGET) A SET OF ITEMS IN THE CONFERENCE                       */
X/******************************************************************************/
Xint                 /* RETURNS: (nothing)     */
Xremember(argc,argv) /* ARGUMENTS:             */
Xint    argc;        /*    Number of arguments */
Xchar **argv;        /*    Argument list       */
X{
X   char       act[MAX_ITEMS];
X   short      j;
X
X   /* Initialize range */
X   rangeinit(&st_glob,act);
X
X   /* Process arguments */
X   if (argc<2) rangetoken("all",&st_glob.opt_flags,act,sum,&st_glob);
X   else        range(argc,argv,&st_glob.opt_flags,act,sum,&st_glob,0);
X
X   /* Process items in specified range */
X   for (j=st_glob.i_first; j<=st_glob.i_last && !(status & S_INT); j++) {
X      if (!act[j-1] || !sum[j-1].flags) continue;
X      part[j-1].nr   = abs(part[j-1].nr);
X      sum[j-1].flags &= ~IF_FORGOTTEN;
X   }
X   return 1;
X}
X
X/******************************************************************************/
X/* MARK A SET OF ITEMS AS BEING SEEN                                          */
X/******************************************************************************/
Xint                /* RETURNS: (nothing)     */
Xfixseen(argc,argv) /* ARGUMENTS:             */
Xint    argc;       /*    Number of arguments */
Xchar **argv;       /*    Argument list       */
X{
X   char       act[MAX_ITEMS];
X   short      j;
X
X   rangeinit(&st_glob,act);
X   refresh_sum(0,confidx,sum,part,&st_glob);
X
X   if (argc<2) {
X      rangetoken("all",&st_glob.opt_flags,act,sum,&st_glob);
X   } else { /* Process args */
X      range(argc,argv,&st_glob.opt_flags,act,sum,&st_glob,0);
X   }
X
X   /* Process items */
X   for (j=st_glob.i_first; j<=st_glob.i_last && !(status & S_INT); j++) {
X      if (cover(j,confidx,st_glob.opt_flags,act[j-1],sum,part,&st_glob)) {
X         part[j-1].nr   = sum[j-1].nr;
X         time(&( part[j-1].last ));     /* or sum.last? */
X      }
X   }
X   return 1;
X}
END_OF_FILE
if test 32474 -ne `wc -c <'item.c'`; then
    echo shar: \"'item.c'\" unpacked with wrong size!
fi
# end of 'item.c'
fi
echo shar: End of archive 5 \(of 5\).
cp /dev/null ark5isdone
MISSING=""
for I in 1 2 3 4 5 ; do
    if test ! -f ark${I}isdone ; then
	MISSING="${MISSING} ${I}"
    fi
done
if test "${MISSING}" = "" ; then
    echo You have unpacked all 5 archives.
    rm -f ark[1-9]isdone
else
    echo You still need to unpack the following archives:
    echo "        " ${MISSING}
fi
##  End of shell archive.
exit 0
