  echo 'File elm24JPpatch-0.35/jp.patch-0.35 is complete' &&
  echo 'uudecoding file elm24JPpatch-0.35/jp.patch-0.35' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 1217170295 'elm24JPpatch-0.35/jp.patch-0.35' &&
  chmod 0640 'elm24JPpatch-0.35/jp.patch-0.35' ||
  echo 'restore of elm24JPpatch-0.35/jp.patch-0.35 failed'
  shar_count="`wc -c < 'elm24JPpatch-0.35/jp.patch-0.35'`"
  test 81380 -eq "$shar_count" ||
    echo "elm24JPpatch-0.35/jp.patch-0.35: original size 81380, current size $shar_count"
  rm -f _sharnew.tmp
fi
# ============= elm24JPpatch-0.35/lib/fflush.c ==============
if test ! -d 'elm24JPpatch-0.35/lib'; then
  echo 'x - creating directory elm24JPpatch-0.35/lib'
  mkdir 'elm24JPpatch-0.35/lib'
fi
if test -f 'elm24JPpatch-0.35/lib/fflush.c' && test X"$1" != X"-c"; then
  echo 'x - skipping elm24JPpatch-0.35/lib/fflush.c (file already exists)'
  rm -f _sharnew.tmp
else
  > _sharnew.tmp
  echo 'x - extracting elm24JPpatch-0.35/lib/fflush.c (text)'
  sed 's/^X//' << 'SHAR_EOF' > 'elm24JPpatch-0.35/lib/fflush.c' &&
X/* do not include "config.h" */
X/*
X** Fflush routine for ultrix.	(95/05/25, wataru@exa.onlab.ntt.jp)
X**
X** In case of non POSIX environment, fflush(read_only_file)
X** causes ferror(read_only_file).
X*/
X
X#ifdef ultrix
X#include <stdio.h>
X
Xstatic char SCCSid[]=
X	"@(#) fflush.c for ultrix, 95/05/25, wataru@exa.onlab.ntt.jp";
X
XFflush(file)
XFILE *file;
X{
X  static int ret;
X
X  if ( !ferror(file) ) {
X    if (file != NULL) {
X      ret = fflush(file);
X    }
X    if ((ret == EOF) && ferror(file)){
X      /* case of fflushing readonly file. */
X      clearerr(file);
X    }
X  }
X  return(ret);
X}
X#endif
SHAR_EOF
  $shar_touch -am 0605021495 'elm24JPpatch-0.35/lib/fflush.c' &&
