fi
# ============= elm24JPpatch-0.35/src/guessesc.c ==============
if test ! -d 'elm24JPpatch-0.35/src'; then
  echo 'x - creating directory elm24JPpatch-0.35/src'
  mkdir 'elm24JPpatch-0.35/src'
fi
if test -f 'elm24JPpatch-0.35/src/guessesc.c' && test X"$1" != X"-c"; then
  echo 'x - skipping elm24JPpatch-0.35/src/guessesc.c (file already exists)'
  rm -f _sharnew.tmp
else
  > _sharnew.tmp
  echo 'x - extracting elm24JPpatch-0.35/src/guessesc.c (text)'
  sed 's/^X//' << 'SHAR_EOF' > 'elm24JPpatch-0.35/src/guessesc.c' &&
X/*
X** guess kin/kout from rest 2 character and fill ESC.
X**
X**	92/10/22, wataru	created
X**	92/10/29, wataru	minor bug fix.
X*/
X#include "headers.h"
X#include <ctype.h>
X#include <stdio.h>
X
X#ifdef  XN_SUBJ
XGuessEsc(s)
Xchar *s;
X{
X/*	static char SCCSid[]="@(#) guessesc.c v1.1 92/10/29 wataru";*/
X	static char SCCSid[]="@(#) guessesc.c v1.2 92/12/02 wataru";
X	int i, j;
X	char *s_0, *s_e, *sp;
X	static char Kio_23[4][3]={"$@","$B","(J","(B"};
X	static int  ipKio_23[4]; /* int pointer for Kio_23 */
X	static int  nKio_23=3;	/* sorts of Kio_23*/
X	static int IsPrevEsc=0; /* Is previous char ESC-char? */
X
X	s_0 = s;
X	s_e = s + strlen(s);
X	
X	IsPrevEsc=0;
X
X        for ( i = 0; i <= nKio_23; i++ ) {
X	    ipKio_23[i] = 0;
X        }
X
X	while (*s != '\0')
X	{
X		/*
X		** Convert "\033"(in character) to '\033'(esc-code) 
X		**			(92/10/21, wataru)
X		*/
X		{
X		    for ( i = 0; i <= nKio_23; i++ ) {
X			if ( *s == Kio_23[i][ipKio_23[i]] 
X						&& IsPrevEsc != 1 ) {
X			    ipKio_23[i]++;
X			} else {
X			    ipKio_23[i] = 0;
X			}
X		    }
X
X		    for ( i = 0; i <= nKio_23; i++ ) {
X			if ( ipKio_23[i] >= strlen(Kio_23[i]) ) {
X/*			    for ( sp = s_e ; /* fixed 92/10/29, wataru */
X			    for ( sp = s_e + 1; /* 92/21/02, wataru */
X			    	  sp >= s - strlen(Kio_23[i]) + 1;
X			    				   sp-- ) {
X				*( sp + 1 ) = *sp;
X			    }
X			    *(sp + 1) = '\033'; /* Insert ESC character */
X			    /* Fill NULL at the end. */
X			    *(s_e + 2) = '\0'; /* added 92/10/29, wataru */
X			}
X		    }
X		}
X
X		if ( *s == '\033' ) {
X		    IsPrevEsc = 1;
X		}
X
X		++s;
X	}
X}
X#endif /* XN_SUBJ */
SHAR_EOF
  $shar_touch -am 0409232995 'elm24JPpatch-0.35/src/guessesc.c' &&
  chmod 0640 'elm24JPpatch-0.35/src/guessesc.c' ||
  echo 'restore of elm24JPpatch-0.35/src/guessesc.c failed'
  shar_count="`wc -c < 'elm24JPpatch-0.35/src/guessesc.c'`"
  test 1551 -eq "$shar_count" ||
    echo "elm24JPpatch-0.35/src/guessesc.c: original size 1551, current size $shar_count"
  rm -f _sharnew.tmp
fi
# ============= elm24JPpatch-0.35/src/jfep.c ==============
if test -f 'elm24JPpatch-0.35/src/jfep.c' && test X"$1" != X"-c"; then
  echo 'x - skipping elm24JPpatch-0.35/src/jfep.c (file already exists)'
  rm -f _sharnew.tmp
else
  > _sharnew.tmp
  echo 'x - extracting elm24JPpatch-0.35/src/jfep.c (text)'
  sed 's/^X//' << 'SHAR_EOF' > 'elm24JPpatch-0.35/src/jfep.c' &&
X#define GETC	getch()
X#define BEEP	putc('\007',stderr)
X#define LINE_OUT stdout
X
X#include <stdio.h>
X/*
X#include "config.h"
X#include "common.h"
X*/
X#include "curses.h"
X#include "kanji.h"
X
X/*    static char SCCSid[]="@(#) jfep.c v1.1, by W.k. Nov.2,1990.";*/
X/* fixed to check if EOF */
X/*    static char SCCSid[]="@(#) jfep.c v1.1, by W.k.,buchi Apr.1,1991."; */
X	/*"@(#) jfep.c 93/07/15(wataru)";*/
X
X/* renewed jfep.c + added kill-line feature. */
X/* use <curses.c> for set/reset to RAW mode. */
Xstatic char SCCSid[]=
X	"@(#) jfep.c 93/08/25(wataru)";
X
Xint ifkanji1(c)
Xunsigned char c;
X{
X#ifdef SJIS
X   if ( (c >= SJIS_TOP1) && (c <= SJIS_END1) ) {
X	return(1);
X   }
X   else if ( (c >= SJIS_TOP2) && (c <= SJIS_END2) ) {
X	return(1);
X   }
X   else {
X
X	return(0);
X   }
X#else /*SJIS*/
X   if ( c > 0x7f ) {
X	return(1);
X   }
X   else {
X	return(0);
X   }
X#endif /*SJIS*/
X}
X
Xint ifkanji2(c)
Xunsigned char c;
X{
X
X   if ( c > 0x7f ) {
X	return(1);
X   }
X   else {
X	return(0);
X   }
X}
X
Xint ifprint(c)
Xunsigned char c;
X{
X   if ( (c >= PRINT_TOP) && (c <= PRINT_END) ) {
X	return(1);
X   }
X   else {
X	return(0);
X   }
X}
X
Xint cursor_left(cur,byte)
Xint *cur;
Xint byte;
X{
X    int i;
X
X/*    printf("*cur=%d, *byte=%d.\n",(*cur), byte ); */
X
X    if ( (*cur) > byte ) {
X	for ( i = 1; i <= byte; i++) {
X	    (*cur)--;
X	    fprintf(LINE_OUT, MOVE_LEFT);
X	}
X	return(1);
X    }
X    else {
X	BEEP;
X/*	printf("*cur=%d, *byte=%d.\n",(*cur), byte ); */
X	return(0);
X    }
X}
X
Xint cursor_right(cur,endp,byte)
Xint *cur,*endp;
Xint byte;
X{
X    int i;
X
X    if ( (*cur) <= ((*endp) - byte + 1) ) {
X	for ( i = 1; i <= byte; i++ ){
X	    (*cur)++;
X	    fprintf(LINE_OUT, MOVE_RIGHT);
X	}
X	return(1);
X    }
X    else {
X	BEEP;
X	return(0);
X    }
X}
X
Xint insert_char(c,cur,endp,buf,byte)
Xint *cur,*endp;
Xint byte;
Xunsigned char c,buf[];
X{
X    int i;
X
X    for ( i = (*endp); i >= (*cur); i--) {
X	buf[i+byte] = buf[i];
X    }
X    buf[(*cur)] = c;
X    (*endp)++;
X    for ( i = 1; i < byte; i++ ){
X	c = GETC;
X	buf[(*cur)+i] = c;
X	(*endp)++;
X    }
X}
X
Xint delete_char(cur,endp,buf,byte)
Xint *cur,*endp;
Xint byte;
Xunsigned char buf[];
X{
X    int i;
X    int c;
X
X    if ( ((*cur) >= 1) && ((*endp) >= byte) && ((*endp) >= (*cur)) ) {
X	    for ( i = ((*cur) + byte); i <= (*endp); i++) {
X		buf[i-byte] = buf[i];
X	    }
X	    for ( i = 1; i <= byte; i++ ){
X	    	(*endp)--;
X	    }
X	    return(1);
X    }
X    else {
X	BEEP;
X	return(0);
X    }
X}
X
Xreprint_char(cur,endp,bscount,buf)
Xint cur,endp,bscount;
Xunsigned char buf[];
X{
X    int i;
X    int c;
X
X#ifdef JIS
X    if ( (cur <=endp) && (buf[cur-1] > 0x7f) ) {
X	   fprintf(LINE_OUT, KANJI_IN);
X    }
X#endif /*JIS*/
X    for ( i = cur; i <= endp; i++) {
X#ifdef JIS
X	if ( (buf[i-1] <= 0x7f) && (buf[i] > 0x7f) ) {
X	   fprintf(LINE_OUT, KANJI_IN);
X	}
X	else if ( (buf[i-1] > 0x7f) && (buf[i] <= 0x7f) ) {
X	   fprintf(LINE_OUT, KANJI_OUT);
X	}
X#endif /*JIS*/
X	c = buf[i];
X	putc((c & 0x7f),LINE_OUT);
X    }
X#ifdef JIS
X    if ( (cur <= endp) && (buf[endp] > 0x7f)) { /* bug-fix? */
X	   fprintf(LINE_OUT, KANJI_OUT);
X    }
X#endif /*JIS*/
X    for ( i = 0; i < bscount-1; i++ ) {
X	putc(SPC,LINE_OUT);
X    }
X    for ( i = endp+bscount; i > cur; i-- ) {
X      fprintf(LINE_OUT, MOVE_LEFT);
X    }
X}
X
Xstatic unsigned char kflag=(unsigned char)0;
X
Xunsigned char GetChFrmStr(hinbuf)
Xunsigned char **hinbuf;
X{
X    /* changed to accept JIS (by wataru, 03/18/91) */
X    static char SCCSid[]="@(#) GetChFrmStr() 92/11/30, wataru ";
X
X    unsigned int c,c1,c2;
X    static unsigned char Kin[]=KIN, Kout[]=KOUT;
X
X    c = (unsigned int) *(*hinbuf)++;
X
X    while ( c == Kin[0] ) {
X	c1 = *(*hinbuf)++;
X	c2 = *(*hinbuf)++;
X    	if ( c1 == Kin[1] ) {
X    	    if ( c2 == Kin[2] || c2 == Kin[3] ) {
X	    	kflag = (unsigned char) 0x80;
X    	    	c = *(*hinbuf)++;
X    	    }
X    	}
X    	else if ( c1 == Kout[1] ) {
X    	    if ( c2 == Kout[2] || c2 == Kout[3] ) {
X	    	kflag = (unsigned char)0;
X    	    	c = *(*hinbuf)++;
X    	    }
X    	}
X    }
X
X    return((unsigned char) (c | (kflag)) );
X
X}
X
X#ifdef JIS
Xunsigned char JisToEuc(buf,outbuf)
Xunsigned char buf[],outbuf[];
X{
X  /* Jis to Euc (by wataru, 93/10/07) */
X  /*static char SCCSid[]="@(#) JisToEuc() 93/10/07, wataru ";*/
X  /* fix for "\kin xxxxx \kout \kin xxxxx \kout" */
X  static char SCCSid[]="@(#) JisToEuc() 93/12/02, wataru ";
X
X  static unsigned char Kin[]=KIN, Kout[]=KOUT;
X  static unsigned char kflg;
X  unsigned char *poutbuf;
X  int i;
X
X  poutbuf = (unsigned char *)outbuf;
X
X  kflg = (unsigned char )0;
X
X  for ( i = 0; i <= strlen(buf); i++ ) {
X    if ( (buf[i] == Kin[0]) && (i <= strlen(buf)+1) ){
X      if ( (buf[i+1] == Kin[1]) && (i <= strlen(buf)+2) ){
X        if ( buf[i+2] == Kin[2] || buf[i+2] == Kin[3] ) {
X          kflg = (unsigned char) 0x80;
X	  /*i = i + 3;*/
X	  i = i + 2; /* 93/12/02, wataru*/
X    	}
X      } else if ( (buf[i+1] == Kout[1]) && (i <= strlen(buf)+2) ) {
X        if ( buf[i+2] == Kout[2] || buf[i+2] == Kout[3] ) {
X     	  kflg = (unsigned char)0;
X	  /*i = i + 3;*/
X	  i = i + 2; /* 93/12/02, wataru*/
X    	}
X      }
X    } else { /* 93/12/02, wataru*/
X      *poutbuf++ = (unsigned char) (buf[i] | (kflg));
X    }
X  }
X  *poutbuf = '\0';
X}
X
Xvoid EucToJis(buf, outbuf)
Xunsigned char buf[], outbuf[];
X{
X    static unsigned char Kin[]=KIN, Kout[]=KOUT;
X
X    int i, bytes;
X    unsigned char *poutbuf, *pbuf, *pbuf0, *pbuf1;
X
X    pbuf = (unsigned char *)buf;
X    pbuf0 = pbuf;
X    pbuf1 = pbuf + strlen(buf);
X    poutbuf = (unsigned char *)outbuf;
X    bytes = 1;
X    
X    /* case of top of line. */
X    if ( (*pbuf & 0x80) != 0 ) {
X      /* insert kanji-in code. */
X      *poutbuf++ = Kin[0];
X      *poutbuf++ = Kin[1];
X      *poutbuf++ = Kin[2];
X      bytes = 2; /* 2-bytes */
X    } else if ( (*pbuf & 0x80) == 0 ) {
X      /* no need to insert kanji-out code. */
X      bytes = 1; /* 1-byte */
X    }
X
X    if ( pbuf+bytes <= pbuf1 ) {
X      for ( i = 0; i <= bytes-1; i++ ) {
X        *poutbuf++ = ( (*pbuf++) & (unsigned char )0x7f );
X      }
X    }
X
X    for ( pbuf = pbuf0 + bytes; pbuf+bytes <= pbuf1; ) {
X      if ( ( *(pbuf-1) & 0x80) == 0 && (*pbuf & 0x80) != 0 ) {
X        /* insert kanji-in code. */
X	*poutbuf++ = Kin[0];
X	*poutbuf++ = Kin[1];
X	*poutbuf++ = Kin[2];
X	bytes = 2;
X      }	else if ( ( *(pbuf-1) & 0x80) != 0 && (*pbuf & 0x80) == 0 ) {
X        /* insert kanji-out code. */
X	*poutbuf++ = Kout[0];
X	*poutbuf++ = Kout[1];
X	*poutbuf++ = Kout[2];
X	bytes = 1;
X      }
X
X      if ( pbuf+bytes <= pbuf1 ) {
X        for ( i = 0; i <= bytes-1; i++ ) {
X          *poutbuf++ = ( (*pbuf++) & (unsigned char )0x7f );
X        }
X      }
X    }
X
X    /* If ends with Kanji, add kanji-out-code. (93/10/08, wataru) */
X    if ( bytes == 2 ) {
X      /* insert kanji-out code. */
X      *poutbuf++ = Kout[0];
X      *poutbuf++ = Kout[1];
X      *poutbuf++ = Kout[2];
X    }
X
X    /* certify to be end of string. */
X    *poutbuf++ = '\0';
X}
X#endif JIS
X
Xvoid EucToSjis(buf, outbuf)
Xunsigned char buf[], outbuf[];
X{
X    static unsigned char Kin[]=KIN, Kout[]=KOUT;
X
X    int i, bytes;
X    unsigned char *poutbuf, *pbuf, *pbuf0, *pbuf1;
X    unsigned char hi, lo;
X
X    pbuf = (unsigned char *)buf;
X    pbuf0 = pbuf;
X    pbuf1 = pbuf + strlen(buf);
X    poutbuf = (unsigned char *)outbuf;
X    bytes = 1;
X    
X    /* case of top of line. */
X    if ( *pbuf >= 0x80 ) {
X      bytes = 2; /* 2-bytes */
X    } else {
X      /* no need to insert kanji-out code. */
X      bytes = 1; /* 1-byte */
X    }
X
X    if ( pbuf+bytes <= pbuf1 ) {
X      if ( bytes == 2 ) {
X	hi = *pbuf;
X	pbuf++;
X	lo = *pbuf;
X	pbuf++;
X
X	/* euc to sjis, from kanji.c in Ng */
X	hi &= 0x7f;
X	lo &= 0x7f;
X	if (hi >= 0xcf)
X	   hi +=0x80;
X	if((hi % 2) == 0){
X	  hi = (hi - 0x30) / 2 + 0x88;
X	  lo += 0x7e;
X	} else {
X	  if ( lo >= 0x60 )
X	    lo += 0x01;
X	  hi = (hi - 0x31) / 2 + 0x89;
X	  lo += 0x1f;
X	}
X	/* end, from kanji.c in Ng. */
SHAR_EOF
  : || echo 'restore of elm24JPpatch-0.35/src/jfep.c failed'
fi
echo 'End of archive part 4'
echo 'File elm24JPpatch-0.35/src/jfep.c is continued in part 5'
echo 5 > _sharseq.tmp
exit 0
