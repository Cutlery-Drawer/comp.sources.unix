  echo 'uudecoding file cdk-4.6.0/man/matrix.3x' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 1229121195 'cdk-4.6.0/man/matrix.3x' &&
