  echo 'uudecoding file cdk-4.6.0/man/radio.3x' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 1229121195 'cdk-4.6.0/man/radio.3x' &&
  chmod 0400 'cdk-4.6.0/man/radio.3x' ||
  echo 'restore of cdk-4.6.0/man/radio.3x failed'
  shar_count="`wc -c < 'cdk-4.6.0/man/radio.3x'`"
  test 6580 -eq "$shar_count" ||
    echo "cdk-4.6.0/man/radio.3x: original size 6580, current size $shar_count"
  rm -f _sharnew.tmp
