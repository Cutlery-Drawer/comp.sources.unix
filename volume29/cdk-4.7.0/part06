SHAR_EOF
  echo 'uudecoding file man/radio.3' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 0503190996 'man/radio.3' &&
  chmod 0444 'man/radio.3' ||
  echo 'restore of man/radio.3 failed'
  shar_count="`wc -c < 'man/radio.3'`"
  test 8653 -eq "$shar_count" ||
    echo "man/radio.3: original size 8653, current size $shar_count"
  rm -f _sharnew.tmp
fi
# ============= man/scale.3 ==============
