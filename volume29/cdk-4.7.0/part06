  echo 'uudecoding file man/matrix.3' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 0503190996 'man/matrix.3' &&
  chmod 0444 'man/matrix.3' ||
  echo 'restore of man/matrix.3 failed'
  shar_count="`wc -c < 'man/matrix.3'`"
  test 13420 -eq "$shar_count" ||
    echo "man/matrix.3: original size 13420, current size $shar_count"
  rm -f _sharnew.tmp
