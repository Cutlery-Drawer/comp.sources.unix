SHAR_EOF
  echo 'uudecoding file man/scale.3' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 0503190996 'man/scale.3' &&
  chmod 0444 'man/scale.3' ||
  echo 'restore of man/scale.3 failed'
  shar_count="`wc -c < 'man/scale.3'`"
  test 9658 -eq "$shar_count" ||
    echo "man/scale.3: original size 9658, current size $shar_count"
  rm -f _sharnew.tmp
fi
