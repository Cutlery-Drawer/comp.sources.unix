  echo 'File radio.c is complete' &&
  echo 'uudecoding file radio.c' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 0503190896 'radio.c' &&
  chmod 0444 'radio.c' ||
  echo 'restore of radio.c failed'
  shar_count="`wc -c < 'radio.c'`"
  test 15203 -eq "$shar_count" ||
    echo "radio.c: original size 15203, current size $shar_count"
  rm -f _sharnew.tmp
fi
