  echo 'uudecoding file selection.c' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 0503190896 'selection.c' &&
  chmod 0444 'selection.c' ||
  echo 'restore of selection.c failed'
  shar_count="`wc -c < 'selection.c'`"
  test 16736 -eq "$shar_count" ||
    echo "selection.c: original size 16736, current size $shar_count"
  rm -f _sharnew.tmp
fi
# ============= swindow.c ==============
