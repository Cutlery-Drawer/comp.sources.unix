  echo 'File matrix.c is complete' &&
  echo 'uudecoding file matrix.c' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 0503190896 'matrix.c' &&
  chmod 0444 'matrix.c' ||
  echo 'restore of matrix.c failed'
  shar_count="`wc -c < 'matrix.c'`"
  test 42377 -eq "$shar_count" ||
    echo "matrix.c: original size 42377, current size $shar_count"
  rm -f _sharnew.tmp
fi
