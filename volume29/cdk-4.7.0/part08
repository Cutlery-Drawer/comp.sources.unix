  chmod 0444 'menu.c' ||
  echo 'restore of menu.c failed'
  shar_count="`wc -c < 'menu.c'`"
  test 15005 -eq "$shar_count" ||
    echo "menu.c: original size 15005, current size $shar_count"
  rm -f _sharnew.tmp
fi
