M.PI]"B`*+RH*("H@5&AI<R!F=6YC=&EO;B!S971S('1H92!P;W-T+7!R;V-E
M<W,@9G5N8W1I;VXN"B`J+PIV;VED('-E=$-$2TUE;G1R>5!O<W10<F]C97-S
M("A#1$M-14Y44ED@*FUE;G1R>2P@4%)/0T534T9.(&-A;&QB86-K+"!V;VED
M("ID871A*0I["B`@(&UE;G1R>2T^<&]S=%!R;V-E<W-&=6YC=&EO;B`](&-A
M;&QB86-K.PH@("!M96YT<GDM/G!O<W10<F]C97-S1&%T82`](&1A=&$["GT*
`
end
SHAR_EOF
  echo 'uudecoding file mentry.c' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 0503190896 'mentry.c' &&
  chmod 0444 'mentry.c' ||
  echo 'restore of mentry.c failed'
  shar_count="`wc -c < 'mentry.c'`"
  test 24525 -eq "$shar_count" ||
    echo "mentry.c: original size 24525, current size $shar_count"
  rm -f _sharnew.tmp
fi
# ============= menu.c ==============
if test -f 'menu.c' && test X"$1" != X"-c"; then
  echo 'x - skipping menu.c (file already exists)'
  rm -f _sharnew.tmp
else
  > _sharnew.tmp
  echo 'x - extracting menu.c (text)'
  sed 's/^X//' << 'SHAR_EOF' > 'menu.c' &&
#include "cdk.h"
X
/*
X * $Author: glover $
X * $Date: 1996/04/16 05:13:33 $
X * $Revision: 1.44 $
X */
X
/*
X * This creates a new menu widget.
X */
CDKMENU *newCDKMenu (CDKSCREEN *cdkscreen, char *menulist[MAXMENUITEMS][MAXSUBITEMS], int menuItems, int *subsize, int *menuloc, int menuPos, chtype titleAttr, chtype subtitleAttr)
{
X   /* Declare local variables.						*/
X   CDKMENU *menu	= (CDKMENU *)malloc (sizeof (CDKMENU));
X   int rightcount	= menuItems-1;
X   int rightloc		= (cdkscreen->window->_maxx) - 1;
X   int leftloc		= 0;
#ifdef NODYNALLOC
X   int maxwidth[MAXMENUITEMS];
#else
X   int maxwidth[menuItems];
#endif
X   int x, y, max, junk;
X
X   /* Start making a copy of the information.				*/
X   rightcount			= menuItems-1;
X   menu->parent			= cdkscreen->window;
X   menu->menuItems		= menuItems;
X   menu->titleAttr		= titleAttr;
X   menu->subtitleAttr		= subtitleAttr;
X   menu->currentTitle		= 0;
X   menu->currentSubtitle	= 0;
X   menu->lastSelection		= -1;
X   menu->preProcessFunction	= (PROCESSFN)NULL;
X   menu->preProcessData		= (void *)NULL;
X   menu->postProcessFunction	= (PROCESSFN)NULL;
X   menu->postProcessData	= (void *)NULL;
X   menu->menuPos		= menuPos;
X
X   /* Create the pull down menus.					*/
X   for (x=0; x < menuItems; x++)
X   {
X      max = -1;
X      for (y=1; y < subsize[x] ; y++)
X      {
X         if (menuloc[x] == LEFT)
X         {
X            menu->sublist[x][y-1] = char2Chtype (menulist[x][y], &menu->sublistLen[x][y-1], &junk);
X            max	= MAX (max, menu->sublistLen[x][y-1]);
X         }
X         else
X         {
X            menu->sublist[rightcount][y-1] = char2Chtype (menulist[x][y], &menu->sublistLen[rightcount][y-1], &junk);
X            max	= MAX (max, menu->sublistLen[rightcount][y-1]);
X         }
X      }
X
X      if (menuloc[x] == LEFT)
X      {
X         /* Its a menu item on the left, add it to the left side 	*/
X         /* of the menu.						*/
X         menu->title[x]		= char2Chtype (menulist[x][0], &menu->titleLen[x], &junk);
X         menu->subsize[x]	= subsize[x] - 1;
X         maxwidth[x] 		= max+1;
X         if (menu->menuPos == BOTTOM)
X         {
X            menu->titleWin[x]	= subwin (cdkscreen->window, 1, menu->titleLen[x]+2, LINES-1, leftloc);
X            menu->pullWin[x]	= subwin (cdkscreen->window, subsize[x]+1, max+2, LINES-subsize[x]-2, leftloc);
X         }
X         else
X         {
X            menu->titleWin[x]	= subwin (cdkscreen->window, 1, menu->titleLen[x]+2, 0, leftloc);
X            menu->pullWin[x]	= subwin (cdkscreen->window, subsize[x]+1, max+2, 1, leftloc);
X         }
X         leftloc		+= menu->titleLen[x] + 1;
X
X         /* Allow the windows to use the keypad.			*/
X         keypad (menu->titleWin[x], TRUE);
X         keypad (menu->pullWin[x], TRUE);
X      }
X      else 
X      {
X         /* Its a menu item on the right, add it to the right side	*/
X         /* of the menu.						*/
X         rightloc			-= max + 3;
X         menu->title[rightcount]	= char2Chtype (menulist[x][0], &menu->titleLen[rightcount], &junk);
X         menu->subsize[rightcount]	= subsize[x] - 1;
X         maxwidth[rightcount] 		= max+1;
X         if (menu->menuPos == BOTTOM)
X         {
X            menu->titleWin[rightcount]	= subwin (cdkscreen->window, 1, menu->titleLen[x]+2, LINES-1, rightloc);
X            menu->pullWin[rightcount]	= subwin (cdkscreen->window, subsize[x]+1, max+2, LINES-subsize[x]-2, rightloc);
X         }
X         else
X         {
X            menu->titleWin[rightcount]	= subwin (cdkscreen->window, 1, menu->titleLen[x]+2, 0, rightloc);
X            menu->pullWin[rightcount]	= subwin (cdkscreen->window, subsize[x]+1, max+2, 1, rightloc);
X         }
X
X         /* Allow the windows to use the keypad.			*/
X         keypad (menu->titleWin[rightcount], TRUE);
X         keypad (menu->pullWin[rightcount], TRUE);
X         rightcount--;
X      }
X   }
X
X   /* Clean the key bindings.						*/
X   cleanCDKObjectBindings (vMENU, menu);
X
X   /* Register this baby.						*/
X   registerCDKObject (cdkscreen, vMENU, menu);
X
X   /* Return the menu object.						*/
X   return (menu);
}
X
/*
X * This activates the CDK Menu.
X */
int activateCDKMenu (CDKMENU *menu, chtype *actions)
{
X   /* Declare local variables.						*/
X   chtype input;
X   int ret;
X
X   /* Draw in the screen.						*/
X   refreshCDKScreen (menu->screen);
X
X   /* Display the menu titles.						*/
X   drawCDKMenu(menu);
X
X   /* Turn the cursor off.						*/
#ifndef AIX
X   curs_set (0);
#endif
X
X   /* Highlight the current title and window.				*/
X   drawCDKMenuSubwin (menu);
X
X   /* If the input string is NULL, this is an interactive activate.	*/
X   if (actions == (chtype *)NULL)
X   {
X      /* Start taking input from the keyboard.				*/
X      for (;;)
X      {
X         input = wgetch (menu->titleWin[menu->currentTitle]);
X
X         /* Inject the character into the widget.			*/
X         ret = injectCDKMenu (menu, input);
X         if (ret != -2)
X         {
X            return ret;
X         }
X      }
X   }  
X   else
X   {
X      int count = chlen (actions);
X      int x = 0;
X
X      for (x=0; x < count; x++)
X      {
X         ret = injectCDKMenu (menu, actions[x]);
X         if (ret != -2)
X         {
X            return ret;
X         }
X      }
X   }
X   return -2;
}
X
/*
X * This injects a character into the menu widget.
X */
int injectCDKMenu (CDKMENU *menu, chtype input)
{
X   /* Declare local variables.						*/
X   int ppReturn = 1;
X
X   /* Check if there is a pre-process function to be called.		*/
X   if (menu->preProcessFunction != (PROCESSFN)NULL)
X   {
X      /* Call the pre-process function.					*/
X      ppReturn = ((PROCESSFN)(menu->preProcessFunction)) (vMENU, menu, menu->preProcessData, input);
X   }
X
X   /* Should we continue?						*/
X   if (ppReturn != 0)
X   {
X      /* Check for key bindings.						*/
X      if (! checkCDKObjectBind (vMENU, menu, input))
X      {
X         switch (input)
X         {
X            case KEY_LEFT :
X                 /* Erase the menu sub-window				*/
X                 eraseCDKMenuSubwin (menu);
X                 refreshCDKScreen (menu->screen);
X   
X                 /* Set the values.					*/
X                 menu->currentSubtitle = 0;
X                 if (menu->currentTitle == 0)
X                 {
X                    menu->currentTitle = menu->menuItems - 1;
X                 }
X                 else
X                 {
X                    menu->currentTitle--;
X                 }
X   
X                 /* Draw the new menu sub-window.			*/
X                 drawCDKMenuSubwin (menu);
X                 break;
X   
X            case KEY_RIGHT : case KEY_TAB :
X                 /* Erase the menu sub-window				*/
X                 eraseCDKMenuSubwin (menu);
X                 refreshCDKScreen (menu->screen);
X   
X                 /* Set the values.					*/
X                 menu->currentSubtitle = 0;
X                 if (menu->currentTitle == menu->menuItems-1)
X                 {
X                       menu->currentTitle = 0;
X                 }
X                 else
X                 {
X                    menu->currentTitle++;
X                 }
X   
X                 /* Draw the new menu sub-window.			*/
X                 drawCDKMenuSubwin (menu);
X                 break;
X   
X            case KEY_UP :
X                 /* Erase the old subtitle.				*/
X                 writeChtype (menu->pullWin[menu->currentTitle],
X   				1, menu->currentSubtitle+1,
X   				menu->sublist[menu->currentTitle][menu->currentSubtitle],
X   				HORIZONTAL,
X   				0, menu->sublistLen[menu->currentTitle][menu->currentSubtitle]);
X   
X                 /* Set the values.					*/
X                 if (menu->currentSubtitle == 0)
X                 {
X                    menu->currentSubtitle	= menu->subsize[menu->currentTitle] - 1;
X                 }
X                 else
X                 {
X                    menu->currentSubtitle--;
X                 }
X   
X                 /* Draw the new sub-title.				*/
X                 writeChtypeAttrib (menu->pullWin[menu->currentTitle],
X   					1, menu->currentSubtitle+1,
X   					menu->sublist[menu->currentTitle][menu->currentSubtitle],
X   					menu->subtitleAttr,
X   					HORIZONTAL,
X   					0, menu->sublistLen[menu->currentTitle][menu->currentSubtitle]);
X                 wrefresh (menu->pullWin[menu->currentTitle]);
X                 break;
X   
X            case KEY_DOWN : case SPACE :
X                 /* Erase the old subtitle.				*/
X                 writeChtype (menu->pullWin[menu->currentTitle],
X   				1, menu->currentSubtitle+1,
X   				menu->sublist[menu->currentTitle][menu->currentSubtitle],
X   				HORIZONTAL,
X   				0, menu->sublistLen[menu->currentTitle][menu->currentSubtitle]);
X   
X                 /* Set the values.					*/
X                 if (menu->currentSubtitle == menu->subsize[menu->currentTitle]-1)
X                 {
X                    menu->currentSubtitle	= 0;
X                 }
X                 else
X                 {
X                    menu->currentSubtitle++;
X                 }
X   
X                 /* Draw the new sub-title.				*/
X                 writeChtypeAttrib (
X   				menu->pullWin[menu->currentTitle],
X   				1, menu->currentSubtitle+1,
X   				menu->sublist[menu->currentTitle][menu->currentSubtitle],
X   				menu->subtitleAttr,
X   				HORIZONTAL,
X   				0, menu->sublistLen[menu->currentTitle][menu->currentSubtitle]);
X                 wrefresh (menu->pullWin[menu->currentTitle]);
X                 break;
X    
X            case KEY_RETURN :
X                 cleanUpMenu (menu);
X                 menu->lastSelection = ((menu->currentTitle * 100) + menu->currentSubtitle);
X                 return menu->lastSelection;
X                 break;
X   
X            case CDK_NEXT : case KEY_ESC :
X                 cleanUpMenu (menu);
X                 menu->lastSelection = -1;
X                 return menu->lastSelection;
X                 break;
X        
X            case CDK_REFRESH :
X                 refreshCDKScreen (menu->screen);
X                 break;
X         }
X      }
X
X      /* Should we call a post-process?					*/
X      if (menu->postProcessFunction != (PROCESSFN)NULL)
X      {
X         ((PROCESSFN)(menu->postProcessFunction)) (vMENU, menu, menu->postProcessData, input);
X      }
X   }
X   return (-2);
}
X
/* This draws a menu item subwindow					*/
void drawCDKMenuSubwin (CDKMENU *menu)
{
X   /* Declare local variables.						*/
X   int x;
X
X   /* Box the window							*/
X   box (menu->pullWin[menu->currentTitle], ACS_VLINE, ACS_HLINE);
X   if (menu->menuPos == BOTTOM)
X   {
X      mvwaddch (menu->pullWin[menu->currentTitle],
X		menu->subsize[menu->currentTitle]+1, 0, ACS_LTEE);
X   }
X   else
X   {
X      mvwaddch (menu->pullWin[menu->currentTitle], 0, 0, ACS_LTEE);
X   }
X
X   /* Draw in the items.						*/
X   for (x=0; x < menu->subsize[menu->currentTitle] ; x++)
X   {
X      writeChtype (menu->pullWin[menu->currentTitle],
X			1, x+1,
X			menu->sublist[menu->currentTitle][x],
X			HORIZONTAL,
X			0, menu->sublistLen[menu->currentTitle][x]);
X   }
X
X   /* Highlight the current sub-menu item.				*/
X   writeChtypeAttrib (menu->pullWin[menu->currentTitle],
X			1, menu->currentSubtitle+1,
X			menu->sublist[menu->currentTitle][menu->currentSubtitle],
X			menu->subtitleAttr, HORIZONTAL, 0,
X			menu->sublistLen[menu->currentTitle][menu->currentSubtitle]);
X   wrefresh (menu->pullWin[menu->currentTitle]);
X
X   /* Highlight the title.						*/
X   writeChtypeAttrib (menu->titleWin[menu->currentTitle],
X			0, 0, menu->title[menu->currentTitle],
X			menu->titleAttr, HORIZONTAL, 0,
X			menu->titleLen[menu->currentTitle]);
X   wrefresh (menu->titleWin[menu->currentTitle]);
}
X
/* This erases a menu item subwindow.					*/
void eraseCDKMenuSubwin (CDKMENU *menu)
{
X   /* Erase the sub-menu.						*/
X   werase (menu->pullWin[menu->currentTitle]);
X   wrefresh (menu->pullWin[menu->currentTitle]);
X
X   /* Redraw the sub-menu title.					*/
X   writeChtype (menu->titleWin[menu->currentTitle],
X		0, 0, menu->title[menu->currentTitle],
X		HORIZONTAL,
X		0, menu->titleLen[menu->currentTitle]);
X   wrefresh (menu->titleWin[menu->currentTitle]);
}
X
/*
X * This function draws the menu.
X */
void drawCDKMenu (CDKMENU *menu)
{
X   /* Declare local variables.						*/
X   int x	= 0;
X
X   /* Erase the old object.						*/
X   eraseCDKMenu (menu);
X
X   /* Draw in the menu titles.						*/
X   for (x=0; x < menu->menuItems; x++)
X   {
X      writeChtype (menu->titleWin[x], 0, 0, menu->title[x], HORIZONTAL, 0, menu->titleLen[x]);
X      wrefresh (menu->titleWin[x]);
X   }
}
X
/*
X * This function destroys a menu widget.
X */
void destroyCDKMenu (CDKMENU *menu)
{
X   /* Declare local variables.						*/
X   int x	= 0;
X   int y	= 0;
X
X   /* Erase the object.							*/
X   eraseCDKMenu (menu);
X
X   /* Clean up both the winodws and the char pointers.			*/
X   for (x=0; x < menu->menuItems; x++)
X   {
X      /* Clean the windows.						*/
X      delwin (menu->titleWin[x]);
X      delwin (menu->pullWin[x]);
X
X      /* Delete the character pointers.					*/
X      freeChtype (menu->title[x]);
X      for (y=0; y < menu->subsize[x] ; y++)
X      {
X         freeChtype (menu->sublist[x][y]);
X      }
X   }
X
X   /* Unregister this object.						*/
X   unregisterCDKObject (vMENU, menu);
X
X   /* Finish cleaning up.						*/
X   free (menu);
}
X
/*
X * This function erases the menu widget from the screen.
X */
void eraseCDKMenu (CDKMENU *menu)
{
X   /* Declare local variables.						*/
X   int x	= 0;
X
X   /* Erase the menu items.						*/
X   for (x=0 ; x < menu->menuItems; x++)
X   {
X      werase (menu->titleWin[x]);
X      wrefresh (menu->titleWin[x]);
X      werase (menu->pullWin[x]);
X      wrefresh (menu->pullWin[x]);
X   }
}
X
/*
X * This sets certain features of the menu.
X */
void setCDKMenu (CDKMENU *menu, int menuitem, int submenuitem, chtype titlehighlight, chtype subtitlehighlight)
{
X   /* Set the menu item position values.			*/
X   if (menuitem < 0)
X   {
X      menu->currentTitle	= 0;
X   }
X   else if (menuitem >= menu->menuItems)
X   {
X      menu->currentTitle	= menu->menuItems - 1;
X   }
X   else
X   {
X      menu->currentTitle	= menuitem;
X   }
X   if (submenuitem < 0)
X   {
X      menu->currentSubtitle	= 0;
X   }
X   else if (submenuitem >= menu->subsize[menu->currentTitle])
X   {
X      menu->currentSubtitle	= menu->subsize[menu->currentTitle]-1;
X   }
X   else
X   {
X      menu->currentSubtitle	= submenuitem;
X   }
X
X   /* Set the attributes.					*/
X   menu->titleAttr		= titlehighlight;
X   menu->subtitleAttr		= subtitlehighlight;
}
X
/* 
X * This exits the menu.
X */
void cleanUpMenu (CDKMENU *menu)
{
X   /* Reset the cursor.							*/
#ifndef AIX
X   curs_set (1);
#endif
X
X   /* Erase the sub-menu.						*/
X   eraseCDKMenuSubwin (menu);
X   wrefresh (menu->pullWin[menu->currentTitle]);
X
X   /* Refresh the screen.						*/
X   refreshCDKScreen (menu->screen);
}
X
/*
X * This function sets the pre-process function.
X */
void setCDKMenuPreProcess (CDKMENU *menu, PROCESSFN callback, void *data)
{
X   menu->preProcessFunction = callback;
X   menu->preProcessData = data;
}
X 
/*
X * This function sets the post-process function.
X */
void setCDKMenuPostProcess (CDKMENU *menu, PROCESSFN callback, void *data)
{
X   menu->postProcessFunction = callback;
X   menu->postProcessData = data;
}
SHAR_EOF
