  echo 'uudecoding file cdk4.8.0/radio.c' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 0709111596 'cdk4.8.0/radio.c' &&
  chmod 0444 'cdk4.8.0/radio.c' ||
  echo 'restore of cdk4.8.0/radio.c failed'
  shar_count="`wc -c < 'cdk4.8.0/radio.c'`"
  test 15639 -eq "$shar_count" ||
    echo "cdk4.8.0/radio.c: original size 15639, current size $shar_count"
  rm -f _sharnew.tmp
fi
