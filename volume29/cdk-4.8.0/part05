  echo 'uudecoding file cdk4.8.0/scroll.c' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 0709111596 'cdk4.8.0/scroll.c' &&
  chmod 0444 'cdk4.8.0/scroll.c' ||
  echo 'restore of cdk4.8.0/scroll.c failed'
  shar_count="`wc -c < 'cdk4.8.0/scroll.c'`"
  test 22218 -eq "$shar_count" ||
    echo "cdk4.8.0/scroll.c: original size 22218, current size $shar_count"
  rm -f _sharnew.tmp
fi
# ============= cdk4.8.0/selection.c ==============
if test -f 'cdk4.8.0/selection.c' && test X"$1" != X"-c"; then
  echo 'x - skipping cdk4.8.0/selection.c (file already exists)'
  rm -f _sharnew.tmp
