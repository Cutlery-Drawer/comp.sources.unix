  echo 'uudecoding file cdk4.8.0/selection.c' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 0709111596 'cdk4.8.0/selection.c' &&
  chmod 0444 'cdk4.8.0/selection.c' ||
  echo 'restore of cdk4.8.0/selection.c failed'
  shar_count="`wc -c < 'cdk4.8.0/selection.c'`"
  test 17152 -eq "$shar_count" ||
    echo "cdk4.8.0/selection.c: original size 17152, current size $shar_count"
  rm -f _sharnew.tmp
