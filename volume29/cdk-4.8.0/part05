if test -f 'cdk4.8.0/swindow.c' && test X"$1" != X"-c"; then
  echo 'x - skipping cdk4.8.0/swindow.c (file already exists)'
  rm -f _sharnew.tmp
else
  > _sharnew.tmp
  echo 'x - extracting cdk4.8.0/swindow.c (binary)'
  sed 's/^X//' << 'SHAR_EOF' > _sharuue.tmp &&
begin 600 cdk4.8.0/swindow.c
M(VEN8VQU9&4@(F-D:RYH(@H*+RH*("H@)$%U=&AO<CH@9VQO=F5R("0*("H@
M)$1A=&4Z(#$Y.38O,#<O,#4@,#$Z,S,Z,#<@)`H@*B`D4F5V:7-I;VXZ(#$N
M,S8@)`H@*B\*"B\J"B`J(%1H:7,@9G5N8W1I;VX@8W)E871E<R!A('-C<F]L
SHAR_EOF
  : || echo 'restore of cdk4.8.0/swindow.c failed'
fi
echo 'End of archive part 5'
echo 'File cdk4.8.0/swindow.c is continued in part 6'
echo 6 > _sharseq.tmp
exit 0
