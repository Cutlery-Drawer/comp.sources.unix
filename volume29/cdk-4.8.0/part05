  chmod 0444 'cdk4.8.0/matrix.c' ||
  echo 'restore of cdk4.8.0/matrix.c failed'
  shar_count="`wc -c < 'cdk4.8.0/matrix.c'`"
  test 44021 -eq "$shar_count" ||
    echo "cdk4.8.0/matrix.c: original size 44021, current size $shar_count"
