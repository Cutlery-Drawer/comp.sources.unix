  echo 'uudecoding file cdk4.8.0/man/matrix.3' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 0709111596 'cdk4.8.0/man/matrix.3' &&
