  echo 'uudecoding file cdk4.8.0/man/selection.3' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 0709111596 'cdk4.8.0/man/selection.3' &&
  chmod 0444 'cdk4.8.0/man/selection.3' ||
  echo 'restore of cdk4.8.0/man/selection.3 failed'
  shar_count="`wc -c < 'cdk4.8.0/man/selection.3'`"
  test 10001 -eq "$shar_count" ||
    echo "cdk4.8.0/man/selection.3: original size 10001, current size $shar_count"
  rm -f _sharnew.tmp
fi
