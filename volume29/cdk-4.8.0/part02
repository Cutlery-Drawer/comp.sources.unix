#!/bin/sh
# This is `cdk_.02' (part 2 of a multipart archive).
# Do not concatenate these parts, unpack them in order with `/bin/sh'.
# File `cdk4.8.0/man/calendar.3' is being continued...
#
touch -am 1231235999 $$.touch >/dev/null 2>&1
if test ! -f 1231235999 && test -f $$.touch; then
  shar_touch=touch
else
  shar_touch=:
  echo
  echo 'WARNING: not restoring timestamps.  Consider getting and'
  echo "installing GNU \`touch', distributed in GNU File Utilities..."
  echo
fi
rm -f 1231235999 $$.touch
#
if test ! -r _sharseq.tmp; then
  echo 'Please unpack part 1 first!'
  exit 1
fi
shar_sequence=`cat _sharseq.tmp`
if test "$shar_sequence" != 2; then
  echo "Please unpack part $shar_sequence next!"
  exit 1
fi
if test ! -f _sharnew.tmp; then
  echo 'x - still skipping cdk4.8.0/man/calendar.3'
else
  echo 'x - continuing file cdk4.8.0/man/calendar.3'
  sed 's/^X//' << 'SHAR_EOF' >> '_sharuue.tmp' &&
M97AI=%1Y<&5<9C$@=&\@=&AE('9A;'5E(&]F(%QF-'9%4T-!4$5?2$E47&8Q
M+@I#=')L+4P@("`@("!2969R97-H97,@=&AE('-C<F5E;BX*+F9I"BY210HN
M4T@@4T5%($%,4T\*+D)2(&-D:R`H,RDL"BY"4B!C9&M?8FEN9&EN9R`H,RDL
M"BY"4B!C9&M?9&ES<&QA>2`H,RDL"BY"4B!C9&M?<V-R965N("@S*0HN4T@@
M3D]415,*+E!0"E1H92!H96%D97(@9FEL92!<9C0\8V1K+F@^7&8Q(&%U=&]M
M871I8V%L;'D@:6YC;'5D97,@=&AE(&AE861E<B!F:6QE<PI<9C0\8W5R<V5S
M+F@^7&8Q+"!<9C0\<W1D;&EB+F@^7&8Q+"!<9C0\<W1R:6YG+F@^7&8Q+"!<
M9C0\8W1Y<&4N:#Y<9C$L"EQF-#QU;FES=&0N:#Y<9C$L(%QF-#QD:7)E;G0N
M:#Y<9C$L(%QF-#QT:6UE+F@^7&8Q+"!<9C0\97)R;F\N:#Y<9C$L"EQF-#QP
M=V0N:#Y<9C$L(%QF-#QG<G`N:#Y<9C$L(%QF-#QS>7,O<W1A="YH/EQF,2P@
M86YD(%QF-#QS>7,O='EP97,N:#Y<9C$N"E1H92!<9C0\8W5R<V5S+F@^7&8Q
M(&AE861E<B!F:6QE(&EN8VQU9&5S(%QF-#QS=&1I;RYH/EQF,2!A;F0@7&8T
M/'5N8W1R;"YH/EQF,2X*+E!0"DEF('EO=2!H879E(%QF-$YC=7)S97-<9C$@
M:6YS=&%L;&5D(&]N('EO=7(@;6%C:&EN92!A9&0@+41.0U524T53('1O('1H
M92`*8V]M<&EL92!L:6YE('1O(&EN8VQU9&4@=&AE($YC=7)S97,@:&5A9&5R
0(&9I;&5S(&EN<W1E860N"G1O
`
end
SHAR_EOF
  echo 'File cdk4.8.0/man/calendar.3 is complete' &&
  echo 'uudecoding file cdk4.8.0/man/calendar.3' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 0709111596 'cdk4.8.0/man/calendar.3' &&
  chmod 0444 'cdk4.8.0/man/calendar.3' ||
  echo 'restore of cdk4.8.0/man/calendar.3 failed'
  shar_count="`wc -c < 'cdk4.8.0/man/calendar.3'`"
  test 11536 -eq "$shar_count" ||
    echo "cdk4.8.0/man/calendar.3: original size 11536, current size $shar_count"
  rm -f _sharnew.tmp
fi
# ============= cdk4.8.0/man/dialog.3 ==============
if test -f 'cdk4.8.0/man/dialog.3' && test X"$1" != X"-c"; then
  echo 'x - skipping cdk4.8.0/man/dialog.3 (file already exists)'
  rm -f _sharnew.tmp
else
  > _sharnew.tmp
  echo 'x - extracting cdk4.8.0/man/dialog.3 (binary)'
  sed 's/^X//' << 'SHAR_EOF' > _sharuue.tmp &&
begin 600 cdk4.8.0/man/dialog.3
