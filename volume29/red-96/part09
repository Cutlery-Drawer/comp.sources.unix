  echo 'uudecoding file Red/red-manual/warning.html' &&
  uudecode _sharuue.tmp < _sharuue.tmp && rm -f _sharuue.tmp &&
  $shar_touch -am 0512222796 'Red/red-manual/warning.html' &&
  chmod 0640 'Red/red-manual/warning.html' ||
