X *	Crispin Goswell @ Rutherford Appleton Laboratory caag@uk.ac.rl.vd
X */
X#include "main.h"
X#include "graphics.h"
X
X#define MOVETO	0x80
X#define BYTE	0xFF
X
X#define FONT_USER	3
X
Xstatic struct show_context show_stack [MAXGSAVES];
X 
