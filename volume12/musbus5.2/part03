X#! /bin/sh
X# Use the contents of the file USER to extract the ordinal user number
X# and map this to that last two characters of a pty name.
X# Assumes: can only support 48 users (numbered 0 ... 47) using ptys
X#	   ttyq0 thru ttyqf, ttyr0 thru ttyrf and ttys0 thru ttysf
Xunum=`cat USER`
Xif test "$unum" -lt 0 -o "$unum" -gt 47
Xthen
X    echo "map_pty: number of users \"$unum\" not legal"
X    exit 1
Xfi
X
Xawk '
XBEGIN	{ maj[0]="q"; maj[1]="r"; maj[2]="s"
X	  hex[0]="a"; hex[1]="b"; hex[2]="c"
X	  hex[3]="d"; hex[4]="e"; hex[5]="f"
X	}
X	{ printf maj[int($1/16)]
X	  min=$1%16
X	  if (min<10) print min
X	  else print hex[min-10]
X	}' USER
X
Xexit 0
End-of-File-Grunt
