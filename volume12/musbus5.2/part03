X#include <stdio.h>
X/*
X * output the tty device name and the matching tty type (from the file ttytype)
X */
X
Xmain(argc, argv)
Xint	argc;
Xchar	*argv[];
X{
X    char	*p;
X    char	*q;
X    FILE	*f;
X    int		c;
X    int		skip = 0;
X    char	*ttyname();
X    char	*getenv();
X
X    p = ttyname(1);		/* stdout, not stdin; unlike tty(1) */
X    freopen("term", "w", stdout);
X    if (p == (char *)0)
X	printf("/dev/unknown ");
X    else
X	printf("%s ", p);
X
X    if (p != (char *)0 && (f = fopen("ttytype", "r")) != NULL) {
X	q = p;
X	while ((c = getc(f)) != EOF) {
X	    if (skip) {
X		if (c == '\n')
X		    skip = 0;
X	    }
X	    else if (*q++ != c) {
X		q = p;
X		skip = 1;
X	    }
X	    else if (*q == '\0') {
X		while ((c = getc(f)) == ' ' || c == '\t')
X		    ;
X		do {
X		    putchar(c);
X		} while ((c = getc(f)) != '\n');
X		putchar('\n');
X		exit(0);
X	    }
X	}
X    }
X
X    p = getenv("TERM");
X    if (p == (char *)0) {
X	    putchar('\n');
X	    exit(1);
X    }
X    printf("%s\n", p);
X    exit(0);
X}
End-of-File-Grunt
