XPATH=XXX:$PATH
Xtermname
XTERM="`cat term | sed 's/.*[ 	]//'`"
Xtty=""
Xexport PATH TERM tty
X% Note: used for pseudo ttys, can only support 48 users using ptys
X%	ttyq0 thru ttyqf, ttyr0 thru ttyrf and ttys0 thru ttysf
Xptn=`map_pty`
X%% 1 small edit
Xrm -f temporary
Xwait
Xkeyb viscr2.dat >/dev/tty$ptn &
Xvi small.txt </dev/pty$ptn
X%% 2 ls
Xls -l
X%% 3 big edit
Xrm -f temporary
Xwait
Xkeyb viscr1.dat >/dev/tty$ptn &
Xvi big.txt </dev/pty$ptn
X%% 4 spell
Xspell big.txt >/tmp/$$
Xdiff big.spell /tmp/$$
Xrm -f /tmp/$$
X%% 5 nroff
Xnroff big.txt >/tmp/$$
Xrm -f /tmp/$$
X%%
Xrm -f term temporary
End-of-File-Grunt
