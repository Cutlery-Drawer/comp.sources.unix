X%W% /bin/sh -ie
XPATH=XXX:$PATH
Xtermname
XTERM="`cat term | sed 's/.*[ 	]//'`"
Xtty=""
Xtmp=/tmp/mus$$bus
Xexport PATH TERM tty
X% Note: used for pseudo ttys, can only support 48 users using ptys
X%	ttyq0 thru ttyqf, ttyr0 thru ttyrf and ttys0 thru ttysf
Xptn=`map_pty`
X%% Ethernet
Xremote=kermit
Xrm -f $tmp
Xrcp $remote:/etc/termcap $tmp
Xrsh $remote -n rm -f $tmp
Xrcp $tmp $remote:$tmp
Xrsh $remote -n diff /etc/termcap $tmp
Xrsh $remote -n rm -f $tmp
X%% small edit
Xwait
Xkeyb viscr2.dat >/dev/tty$ptn &
Xvi small.txt </dev/pty$ptn
Xrm -f temporary
X%% ls
Xls -l /bin
X%% nroff
Xnroff big.txt >$tmp
X%% hanoi
Xhanoi 22
X%% randmem
Xrandmem -s1024 -n4000000
X%% pipe i/o
Xpipe 10000
X%% syscall
Xsyscall 40000
X%% fstime
Xfstime 1000 /tmp
X%% C compiler
Xcc dummy.c
Xrm a.out
X%%
Xrm -f $tmp term
End-of-File-Grunt
